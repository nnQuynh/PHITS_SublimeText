[ Title ]
Dose evaluation for ground contamination

[ Parameters ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =      1000000     # (D=10) number of particles per one batch
 maxbch   =           1     # (D=10) number of batches

set:c1[20000] $ World cylinder radius
set:c2[1.0]   $ Density of soil g/cm3
set:c3[1000]  $ Reflection box size xy
set:c4[800]   $ Scoring box size xy
set:c5[1000]  $ Scoring box height

[ Source ] off
  totfact =     pi*c1*c1     $ per 1 Bq/cm2
   s-type =     1
     proj =   photon
       r0 =     c1
       z0 =    -0.5/c2       $ 0.5g/cm2 depth 
       z1 =    -0.5/c2       $ 0.5g/cm2 
      dir =   all            $ isotropic
   e-type =   28             $ RI soource
       ni =   1
     137Cs    1.0            $ activity is 1 Bq

[ Source ] off
  totfact =     c3*c3
   s-type =     2
     proj =   photon
       x0 =    -0.5*c3
       x1 =     0.5*c3
       y0 =    -0.5*c3
       y1 =     0.5*c3
       z0 =    -0.5/c2       $ 0.5g/cm2 depth 
       z1 =    -0.5/c2       $ 0.5g/cm2 
      dir =   all           # z-direction of beam [isotropic]
   e-type =   28            # Energy distribution. 28 means RI soource
       ni =   1
     137Cs    1.0            $ activity is 1 Bq

[ Source ]
  totfact = 2.2713E+06
   s-type =     17
     file = cross_dmp.out 
    proj =   photon
     dump = -8
             1 2 3 4 5 6 7 8

[ Surface ]
  50  rpp -248.0 248.0 -248.0 248.0  2.0 498.0         $ Object wall interior
  51  rpp -250.0 250.0 -250.0 250.0  0.0 500.0         $ Object wall exterior
  52  rpp -250.0 250.0 -250.0 250.0 -50.0  0.0         $ Soil beneath object
 100  rpp -0.5*c4 0.5*c4 -0.5*c4 0.5*c4 -50 c5 $ for scoring box
 101  px -0.5*c3                               $ for reflection box
 102  px  0.5*c3                               $ for reflection box
 103  py -0.5*c3                               $ for reflection box
 104  py  0.5*c3                               $ for reflection box
  10  sz  100.0 10.0                           $ Detection volume
 900  pz  -50.0                                $ Soil depth down to 50 cm
 901  pz    0.0                                $ Soil surface
 902  pz 20000.0                               $ Sky top up to 200 m
 910  cz  c1                                   $ World cylinder
  
[ Cell ]
 10  1 -1.2e-3  -10                                $ Detection volume 
 11  1 -1.2e-3   10 -50                            $ Air inside wooden hut 
 50  3 -0.64     50 -51                            $ Wood wall
 20  2 -c2     -910 900 -901 -52                   $ Soil bneath object

100  1 -1.2e-3  -910 901 -902 51 $10 #(101 -102 103 -104) $ Air outside reflection boundary
$101  1 -1.2e-3   101 -102 103 -104 901 -902 10 100   $ Air inside reflection boundary
$102  1 -1.2e-3  -100  901 10                       $ Air inside scroing box

200  2 -c2     -910 900 -901  52 $#(101 -102 103 -104)  $ Soil outside reflection boundary
$201  2 -c2      101 -102 103 -104 900 -901 100    $ Soil inside reflection boundary 
$202  2 -c2      -100 -901                          $ Soil inside scoring box 

900 -1           910:-900:902  $ Outer void

[ Volume ]
 reg  vol
 10   4*pi*10.0**3/3.0

[ Material ]
MAT[1]  $ Air  (ICRP144; -0.0012 /cc)
    C  -0.0124
    N  -0.7553 
    O  -0.2318 
    Ar -0.0128 

MAT[2]  $ Soil (ICRP144; -1.0 g/cm3)
    H  -0.0220
    O  -0.5750
    Al -0.0850
    Si -0.2620
    Fe -0.0560

MAT[3]  $ Wood (PNNL-15870Rev2.pdf ; -0.64 g/cm3)
    H  -0.059642
    C  -0.497018
    N  -0.004970
    O  -0.427435
   Mg  -0.001988
    S  -0.004970
    K  -0.001988
   Ca  -0.001988

[ Mat Name Color]
   mat    name   color
    1     Air    lightgray
    2     Soil   camel
    3     Wood   brown

[ T-track ]
 file = dose1.out
 mesh = reg
  reg = 10
 e-type = 2
 ne     = 1
 emin   = 0.0
 emax   = 1000.0
 axis   = reg
 unit   = 1
 multiplier = 1
 part = photon
 mat  mset1
 1    (1.0e-6*3600.0 -200) $ Ambient equivalent dose

[ T-track ]
 file = vdose1.out
 mesh = xyz
 x-type = 2
 nx     =  1
 xmin   = -50.0
 xmax   =  50.0
 y-type = 2
 ny     =  1
 ymin   = -50.0
 ymax   =  50.0
 z-type = 2
 nz     =  20
 zmin   =  0.0
 zmax   =  500.0
 e-type = 2
 ne     = 1
 emin   = 0.0
 emax   = 1000.0
 axis   = z
 angel  = ylin
 gshow  = 1
 epsout = 1
 y-txt = Ambient equivalent dose [uSv/h]
 multiplier = 1
 part = photon
 mat  mset1
 1    (1.0e-6*3600.0 -200) $ Ambient equivalent dose

[ T-track ]
 file = track1.out
 mesh = xyz
 x-type = 2
 nx     =  200
 xmin   = -c1
 xmax   =  c1
 y-type = 2
 ny     =  1
 ymin   = -c1
 ymax   =  c1
 z-type = 2
 nz     =  100
 zmin   = -50.0
 zmax   =  20000.0
 e-type = 2
 ne     = 1
 emin   = 0.0
 emax   = 1000.0
 axis   = xz
 gshow  = 1
 epsout = 1

[ T-track ]
 file = track_inside1.out
 mesh = xyz
 x-type = 2
 nx     =  100
 xmin   = -c3*0.6
 xmax   =  c3*0.6
 y-type = 2
 ny     =  1
 ymin   = -50.0
 ymax   =  50.0
 z-type = 2
 nz     =  200
 zmin   = -50.0
 zmax   =  c5*1.2
 e-type = 2
 ne     = 1
 emin   = 0.0
 emax   = 1000.0
 axis   = xz
 gshow  = 1
 epsout = 1

[ T-cross ] off
 file = cross.out
 mesh = reg
 reg = 1
 r-from  r-to      area
 (101 201) (102 202) 1.0
 output = current
 unit =  1
 axis = reg
 dump = -8
      1 2 3 4 5 6 7 8 

[ End ]
