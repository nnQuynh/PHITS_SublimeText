[ T i t l e ]
Sample input for airshower simulation

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =        1000     # (D=10) number of particles per one batch
 maxbch   =           2     # (D=10) number of batches
 e-mode   =           2     # (D=0) 0: Normal, 1: Event generator mode Ver.1, 2: Ver.2
 negs     =           2     # (D=-1) =-1:original, =0:No, =1:EGS
 mdbatima =        3000     # (D=500) max database size of ATIMA
 maxbnk   =      100000     # (D=10000) maximum bank memory length
 iMeVperU =           1     # (D=0) 0:[MeV] or 1:[MeV/u] is unit of tally output
 deltm    =  1011210.10     # (D=20.12345) max. flight mesh for transport (cm)
 matadd   =           1     # (D=1) 0:no 1:add matt with different den for GG
 icxsni   =           2     # (D=0) 0: Pearlstein-Niita, 1: KUROTAMA, 2: Sato

[ S o u r c e ]
set:c1[10e5]  $ Source radius
  totfact =  pi*c1**2       # (D=1.0) global factor
 infl:{source.inp}
      proj =  proton        # kind of incident nucleus 

