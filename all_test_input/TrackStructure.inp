[ T i t l e ]
input file for lecture about [parameters] section

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =           4     # (D=10) number of particles per one batch
 maxbch   =           2     # (D=10) number of batches
 emin(12) = 1.000000000E-03 # (D=1.d9) cut-off energy of electron (MeV)
 emin(13) = 1.000000000E-03 # (D=1.d9) cut-off energy of positron (MeV)
 negs     =           1     # (D=-1) =-1:original, =0:No, =1:EGS
 etsmax   = 1.000000000E+06 # (D=1.0D-2) Switch energy of electron for track structure mode
 etsmin   = 1.000000000E-06 # (D=1.0D-6) Cutoff energy of electron for track structure mode
 tsmax    = 1.000000000E+06 # (D=1.0D-3) Energy upper limit of ion/proton track structure mode
 tsxcl    = 0               # (D=1) Track structure exclusive mode. 0:on, 1:off

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =   proton        # incident particle species          
       e0 =   300.0001       # energy of beam [MeV/u]
       r0 =   1.0e-5        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -2.99e-5        # minimum position of z-axis [cm]
       z1 =  -2.99e-5        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ]   $ Water
            H         2.0
            O         1.0

MAT[ 2 ]   $ ICRU soft tissue
            H         6.0820000E-02
            C         5.5700000E-03
            N         1.1180000E-03
            O         2.8680000E-02

[ S u r f a c e ]
  10  rpp    -3.0E-5  3.0E-5  -3.0E-5  3.0E-5  -3.0E-5  3.0E-5
  11  so      1.0E-5
  12  px      0.0

[ C e l l ]
  1     1   -1.0   -11     -12
  2     1   -1.0   -10  11 -12
  3     2   -1.0   -11      12
  4     2   -1.0   -10  11  12
 99    -1           10

[track structure]
reg  mID
  1    1
  2    1
  3    0
  4    0

[ T - Interact ]
    title = Number of interaction in reg mesh
     mesh =  reg            # mesh type is region-wise
      reg = 1 2
   e-type =    2            # e-mesh is linear given by emin, emax and ne
     emin =   0.000000      # minimum value of e-mesh points
     emax =   200.0000      # maximum value of e-mesh points
       ne =    1            # number of e-mesh points
     MorP =  prob           # (D=mean) output mode
   maxact =      100        # number of act-mesh points
     axis =  act            # axis of output
     unit =    1            # unit is [1/cm^3/source]
     file = interact.out      # file name of output for the above axis
   output = ts_ioniz_e-exc  # (D=nuclear) ionization and excitation in all track structure modes
     part = all
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
   epsout =    1            # (D=0) generate eps file by ANGEL

[ T - Deposit ]
    title = Energy deposition in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =   2             # x-mesh is linear given by xmin, xmax and nx
     xmin =  -3.E-5         # minimum value of x-mesh points
     xmax =   3.E-5         # maximum value of x-mesh points
       nx =  500            # number of x-mesh points
   y-type =    1            # y-mesh is given by the below data
       ny =    1            # number of y-mesh points
          -1.E-1  1.E-1
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -3.E-5         # minimum value of z-mesh points
     zmax =   3.E-5         # maximum value of z-mesh points
       nz =  500            # number of z-mesh points
     unit =    0
 material =  all            # (D=all) number of specific material
   output =  dose           # total deposit energy
     axis =    xz           # axis of output
     file = deposit.out     # file name of output for the above axis
     part =  all      
    gshow =    3            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
   epsout =    1            # (D=0) generate eps file by ANGEL
    angel = cmum

[ T - Deposit ]
    title = Energy deposition in reg mesh
     mesh =  reg
      reg =  (1 2 3 4)
     unit =    2
 material =  all            # (D=all) number of specific material
   output =  dose           # total deposit energy
     axis =   reg           # axis of output
     file = deposit-reg.out     # file name of output for the above axis
     part =  all      

[ E n d ]

