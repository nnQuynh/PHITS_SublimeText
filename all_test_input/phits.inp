[ T i t l e ]
input file for electron track structure mode

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =           5     # (D=10) number of particles per one batch
 maxbch   =           5     # (D=10) number of batches
 emin(12) = 1.000000000E-03 # (D=1.d9) cut-off energy of electron (MeV)
 emin(13) = 1.000000000E-03 # (D=1.d9) cut-off energy of positron (MeV)
 negs     =           1     # (D=-1) =-1:original, =0:No, =1:EGS
 istdev   =          -2
 etsmax   = 1.000000000     # (D=1.0D-2) Switch energy of electron for track structure mode
 etsmin   = 7.000000000E-06 # (D=1.0D-6) Cutoff energy of electron for track structure mode
 tsmax    = 1000.000000     # (D=1.d-3) maximam energy of proton and ion track structure
 tsxcl    =           1     # (D=0) Track structure exclusixe mode. 0:off, 1:KURBUC, 2:All 
 file(6)  = phits.out       # (D=phits.out) general output file name


[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
    proj =    electron      # kind of incident nucleus
       e0 =   4.0e-3        # energy of beam [MeV/u]
       r0 =   0.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =   0.05E-4       # minimum position of z-axis [cm]
       z1 =   0.05E-4       # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ]   $ Water
            H         2.0
            O         1.0

[ S u r f a c e ]
  10  so      1000.0
  11  sph     0.0  0.0  0.15E-4  0.05E-4

[ C e l l ]
  1     1   -1.0   -11
  2     1   -1.0   -10 #1
 99    -1           10

[track structure]
reg  mID
  1    1
  2    0

[ T - Track ]
    title = Track in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =   2             # x-mesh is linear given by xmin, xmax and nx
     xmin =  -0.15E-4         # minimum value of x-mesh points
     xmax =   0.15E-4         # maximum value of x-mesh points
       nx =  500            # number of x-mesh points
   y-type =    1            # y-mesh is given by the below data
       ny =    1            # number of y-mesh points
          -1.E-1  1.E-1
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =   0.E-5         # minimum value of z-mesh points
     zmax =   0.3E-4        # maximum value of z-mesh points
       nz =  500            # number of z-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1000.0
     unit =    1            # unit is [1/cm^2/source]
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = track.out       # file name of output for the above axis
     part =  all
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
   epsout =    1            # (D=0) generate eps file by ANGEL
    angel = cmnm

[ t - userdefined ]
     file = event.d
  nudtvar = 11
udtvar(1) = 10              # ets-ionization
udtvar(2) = 11              # ets-electronic excitation
udtvar(3) = 12              # ets-dissociative electron attachment
udtvar(4) = 13              # ets-vibrational excitation
udtvar(5) = 14              # ets-phonon excitation
udtvar(6) = 15              # ets-rotation excitation
udtvar(7) = 16              # KURBUC-elastic
udtvar(8) = 17              # KURBUC-ionization
udtvar(9) = 18              # KURBUC-excitation
udtvar(10) = 19             # KURBUC-capture
udtvar(11) = 20             # KURBUC-loss

[END]
