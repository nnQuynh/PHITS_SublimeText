[ T i t l e ]
Test Calculation for [t-point]

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =       10000     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 emin(2)  = 1.000000000E-10 # (D=1.0) cut-off energy of neutron (MeV)
 dmax(2)  =  20.0000000     # (D=emin(2)) data max. energy of neutron (MeV)
 emin(14) = 1.000000000E-03 # (D=1.d9) cut-off energy of photon (MeV)
 dmax(14) =  1000.00000     # (D=emin(14)) data max. energy of photon (MeV)
 igamma   =           2     # (D=0) 0:No, 1:Old, 2:EBITEM, 3:EBITEM+Isomer
 ides     =           1     # (D=0) 0/1 photons will/not produce electrons
 file(6)  = phits.out       # (D=phits.out) general output file name
 file(7)  = c:/phits/data/xsdir.jnd # (D=c:/phits/data/xsdir.jnd) nuclear data input file name

[ S o u r c e ] 
 s-type =   1     
   proj =   neutron
     e0 =   1.0
     z0 =   0.0
     z1 =   0.0
     r0 =   0.0
    dir =   all

[ M a t e r i a l ] 
m1 14N   4.0586E-05
   16O   1.0800E-05

m2 56Fe 1

[surface]  
  1   so    20.0
  2   so   100.0
  3   s    10.0 0.0 50.0 1.0 

[cell] 
  1   2   -7.6       -1
  2   1   -0.00121   -2  1  3
  3   1   -0.00121   -3
  4  -1               2

[t-point] 
  title  = Example of Point Detector
   point = 1
  x   y    z  r0
 10   0   50   1
 part = neutron photon
 e-type = 3
 emin = 1.e-5
 emax = 10
 ne   = 60
 axis = eng
 file = point.out
 epsout = 1
 angel = ymin(1.e-9) ymax(1.e-4)

[t-point] 
  title  = Example of Ring Detector
    ring = 1
 axis  ar  rr  r0
    z  50  10   1
 part = neutron photon
 e-type = 3
 emin = 1.e-5
 emax = 10
 ne   = 60
 axis = eng
 file = ring.out
 epsout = 1
 angel = ymin(1.e-9) ymax(1.e-4)

[ T - T r a c k ]
    title = Track Detection in reg mesh
     mesh =  reg            # mesh type is region-wise
 reg =    3
 part = neutron photon
 e-type = 3
 emin = 1.e-5
 emax = 10
 ne   = 60
 axis = eng
 file = track.out
 epsout = 1
 angel = ymin(1.e-9) ymax(1.e-4)

[ T - track ] 
     mesh = xyz
   x-type = 2
     xmin = -20
     xmax =  20
     nx   = 40
   y-type = 2
     ymin = -1
     ymax =  1
     ny   = 1
   z-type = 2
     zmin = 0
     zmax = 100
     nz   = 100
     part = neutron photon
   e-type =  1
       ne =  1
       0   1.0e5
     axis = xz
     file = track-xz.out
    unit = 1
    epsout = 1
    gshow = 1
