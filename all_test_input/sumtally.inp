[ T i t l e ]
Sample input for sumtally option

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =         100     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 ipnint   =           1     # (D=0) photonuclear GDR, 1:analog, -1:implicit itall
 negs     =           1     # (D=0) =1 EGS photon and electron

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  photon         # kind of incident nucleus          
       e0 =   4.000         # energy of beam [MeV/u]
       r0 =   1.0000        # radius [cm]
       x0 =  -2.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  20.0000        # minimum position of z-axis [cm]
       z1 =  20.0000        # maximum position of z-axis [cm]
      dir =  -1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ]   # ICRU soft tissue
           1H         6.0820000E-02
          12C         5.5700000E-03
          14N         1.1180000E-03
          16O         2.8680000E-02

[ C e l l ]
 1          1 -1.0  1 -2 -11   $ Phantom
98          0      #1    -999  $ Void
99         -1             999  $ Outer region

[ S u r f a c e ]
 1            pz     5.0
 2            pz    15.0
 11           cz     5.0
 999          so   100.0

[ T - T r a c k ]
    title = Track in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -15.00000      # minimum value of x-mesh points
     xmax =   15.00000      # maximum value of x-mesh points
       nx =   60            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -5.000000      # minimum value of y-mesh points
     ymax =   5.000000      # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -10.00000      # minimum value of z-mesh points
     zmax =   30.00000      # maximum value of z-mesh points
       nz =   80            # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = result-r.out    # file name of output for the above axis
     part = photon electron
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1000.0
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat

[END]

