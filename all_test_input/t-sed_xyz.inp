[ T i t l e ]
sample input file of t-sed

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =          10     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 file(6)  = phits.out       # (D=phits.out) general output file name

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  proton         # kind of incident particle         
      dir =   1.0           # z-direction of beam [cosine]
       r0 =   0.            # radius [cm]
       z0 =   0.            # minimum position of z-axis [cm]
       z1 =   0.            # maximum position of z-axis [cm]
       e0 =   150.          # energy of beam [MeV]

[ M a t e r i a l ]
mat[1]    H 2  O 1     $ water

mat[99]    H -2.0/18.0  O  -16.0/18.0  $ 1 g/cm3 water for letmat

[ S u r f a c e ]
  10  so     10.

[ C e l l ]
 100     1 -1.               -10 
 101    -1              10

[ T - S E D ]
    title = Lineal-energy distribution in water in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
       nx =   5            # number of x-mesh points
     xmin =  -5.           # minimum value of x-mesh points
     xmax =   5.           # maximum value of x-mesh points
   y-type =    1            # y-mesh is given by the below data
       ny =    1            # number of y-mesh points
           -5.0  5.0
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
       nz =   1            # number of z-mesh points
     zmin =  -5.           # minimum value of z-mesh points
     zmax =   5.           # maximum value of z-mesh points
  se-type =    3            # e-mesh is log given by emin, emax and ne
    emin  =  1.0000000E-02  # minimum value of e-mesh points
    emax  =   10000.00      # maximum value of e-mesh points
       ne =  180            # number of e-mesh points
  se-unit =    2            # se-unit is [keV/um]
     unit =    7            # unit is z*f(z) [dimensionless]
    cdiam =  1.000000       # (D=1.0) diameter of SE cell [um]
 material =  all            # (D=all) number of specific material
     axis =  sed            # axis of output
     file = y-distribution_xyz.out  # file name of output for the above axis
     part =  all      
   epsout =    1
   letmat =  -99            # (D=0) mat ID for LET, 0:real mat, <0: electron for H2O
    model =    1            # (D=0) Model type 0:old, 1:new

[ E n d ]

