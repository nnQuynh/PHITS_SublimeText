[ T i t l e ]
Sample input for dose calculation for photon therapy

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =      100000     # (D=10) number of particles per one batch
 maxbch   =          20     # (D=10) number of batches
 icrdm    =           1     # (D=0) 0: =icrhi, 1: MWO formula for Deuteron
 dmax(15) =   20.000000     # (D=emin(15)) data max. energy of deuteron (MeV/n)
 ngem     =           2     # (D=0) 0: Default (Ver.1), 1: GEM Ver.1, 2:GEM Ver.2
 igamma   =           3     # (D=2) 0:No, 1:Old, 2:EBITEM, 3:EBITEM+Isomer
 icrdm    =           1     # (D=0) 0: =icrhi, 1: MWO formula for Deuteron

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =   deuteron      # kind of incident nucleus          
       e0 =   15.000        # energy of beam [MeV/u]
       r0 =   0.1000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -20.000        # minimum position of z-axis [cm]
       z1 =  -20.000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ]   # Beam pipe
           Al         1.0

MAT[ 2 ]   # Target
           Li         1.0

MAT[ 3 ]   # moderator
           H          2.0
           O          1.0

MAT[ 4 ]   # reflector
           Be         1.0

MAT[ 5 ]   # shield
           B          4.0
           C          1.0

[Mat name color]
mat name
1 Al
2 Li
3 H2O
4 Be
5 B4C

[ C e l l ]
 1          1 -2.7   1 -2 -101 100  203 $ Beam pipe
 2          2 -0.53  -200               $ Li target
 3          3 -1.00  -201 +200          $ Water moderator
 4          4 -1.85  -202 +201          $ Be reflector
 5          5 -2.52  -203 +202 +201 100 $ B4C Shield 
 6          0        -203 +202 +201 -100$ B4C Shield hole
98          0  #1 203    -999           $ Void
99         -1             999           $ Outer region

[ S u r f a c e ]
 1            pz   -20.0
 2            pz   150.0
 100          cz    10.0
 101          cz    15.0
 200          RPP   -5   5    -5    5   0   2 $ target
 201          RPP   -12  12   -12   12  0  30 $ moderator
 202          RPP   -44  44   -44   44  0  29 $ reflector
 203          RPP   -45  45   -45   45 -1  30 $ Shield   
 999          so  1000.0

[ Forced collisions ]
  part = neutron
   reg   fcl
    1   -1.0

[ T - D e p o s i t ]
    title = Deposit in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin = -50.000000      # minimum value of x-mesh points
     xmax =  50.000000      # maximum value of x-mesh points
       nx =  50             # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin = -50.000000      # minimum value of y-mesh points
     ymax =  50.000000      # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -20.00000      # minimum value of z-mesh points
     zmax =  180.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
     unit =    0            # unit is [Gy/source]
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = deposit.out     # file name of output for the above axis
 material =  all            # (D=all) number of specific material
   output = dose            # only heat is written
   epsout =    1            # (D=0) generate eps file by ANGEL
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
    part  = all proton  deuteron electron  nucleus

[ T - T r a c k ]
    title = Track in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin = -50.000000      # minimum value of x-mesh points
     xmax =  50.000000      # maximum value of x-mesh points
       nx =  50             # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin = -50.000000      # minimum value of y-mesh points
     ymax =  50.000000      # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -20.00000      # minimum value of z-mesh points
     zmax =  180.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = track.out       # file name of output for the above axis
     part = deuteron proton neutron photon
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1000.0
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
	resol =    4 

[ T - D P A ]
    title = dpa in r-z mesh
     mesh =  r-z            # mesh type is xyz scoring mesh
   r-type =    1            # y-mesh is given by the below data
       nr =    1            # number of y-mesh points
            0.0 10.0
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =    0.00000      # minimum value of z-mesh points
     zmax =          2      # maximum value of z-mesh points
       nz =   10            # number of z-mesh points
   output = dpa
     unit =    1
     axis =   z            # axis of output
     file = dpa_r-z.out  # file name of output for the above axis
     part =  all      
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
   epsout =    1            # (D=0) generate eps file by ANGEL

[ T - Y i e l d ]
    title = Production yield of isotopes in region mesh
     mesh =  reg            # mesh type is region-wise
      reg = 1
   volume                   # combined, lattice or level structure 
   non     reg      vol     # reg definition
    1      1         1
 material =  all            # (D=all) number of specific material
   mother =  all            # (D=all) number of specific mother
  nucleus =  all            # (D=all) number of detected nucleus
     unit =    1            # unit is [1/source]
     axis = chart           # axis of output
     file = yield_reg.out   # file name of output for the above axis
     info =    1            # 0-> no, 1-> write magic number and stable nuclei
     part =  all
   output = product         # (D=product) normal yeild
   epsout =    1            # (D=0) generate eps file by ANGEL

[ T - C r o s s ]
    title = Energy distribution in r-z mesh
     mesh =  r-z            # mesh type is xyz scoring mesh
   r-type =    1            # y-mesh is given by the below data
       nr =    1            # number of y-mesh points
            0.0 10.0
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =    0.00000      # minimum value of z-mesh points
     zmax =  150.00000      # maximum value of z-mesh points
       nz =    15           # number of z-mesh points
   e-type =    2            # e-mesh is log given by emin, emax and ne
     emin =   0.000000      # minimum value of e-mesh points
     emax =   100.0000      # maximum value of e-mesh points
       ne =    1            # number of e-mesh points
     unit =    1            # unit is [1/cm^2/source]
     axis =    Z            # axis of output
     file = cross-r-z.out   # file name of output for the above axis
   output = current         # surface crossing flux
     part =  neutron
   epsout =    1            # (D=0) generate eps file by ANGEL

[END]

