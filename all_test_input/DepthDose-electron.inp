[ T i t l e ]  
Depth dose distribution of 30keV electrons into silicon
Figure 22 in JOURNAL OF NUCLEAR SCIENCE AND TECHNOLOGY, VOL. 54, 617-635.
https://doi.org/10.1080/00223131.2017.1297742

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 itall    =           1     # (D=0) 0:no tally at batch, 1:same, 2:different
 maxcas   =      200000     # (D=10) number of particles per one batch
 maxbch   =         10      # (D=10) number of batches
 negs     =           1     # (D=-1) =-1:original, =0:No, =1:EGS
 emin(12) = 1.000000000E-03 # (D=1.d9) cut-off energy of electron (MeV)
 emin(13) = 1.000000000E-03 # (D=1.d9) cut-off energy of positron (MeV)
 emin(14) = 1.000000000E-03 # (D=1.d9) cut-off energy of photon (MeV)
 file(1)  = c:/phits        # (D=c:/phits) PHITS install folder name

set: c1[0.5] $ beam radius

[ S o u r c e ] 
  totfact =   1.0000        # (D=1.0) global factor
   s-type =   1             # mono-energetic axial source
     proj =  electron       # kind of incident particle         
       e0 = 0.030        # energy of beam [MeV]
       r0 =  c1
       z0 = -10.0  #minimum z coordinate (cm)
       z1 = -10.0  #maximum y coordinate (cm), when (z1=z0), rectangle source
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
m1  Si 1.0
set: c4[2.33]  $ density of target [g/cm3]

[ C e l l ]
    1     1 -c4    1 -2 -3 
    99    0      -7 (-1:2:3)
    999   -1      7

[ S u r f a c e ]
    1       pz    0
    2       pz    50
    3       cz    50.0
    7       so    1000

[t-deposit] # depth dose
mesh=r-z
r-type=2
nr=1
rmin=0
rmax=c1
z-type=2
nz=50
zmin=0.0
zmax=0.002
part=all
axis=z
file=deposit-depth.dat
output=dose
unit=1
epsout =    1            # (D=0) generate eps file by ANGEL
# MeV/cm3/source -> Gy cm2
factor = 1/c4*(pi*c1**2)
y-txt = Dose (MeV cm2/g)
angel = ylin ymin(0)

[END]

