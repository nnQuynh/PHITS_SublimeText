[ T i t l e ]
Sample input for dose calculation for charged particle therapy

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =         100     # (D=10) number of particles per one batch
 maxbch   =          20     # (D=10) number of batches
 emin(2)  = 1.000000000E-10 # (D=1.0) cut-off energy of neutron (MeV)
 dmax(2)  =  20.0000000     # (D=emin(2)) data max. energy of neutron (MeV)
 emin(12) = 1.000000000E-00 # (D=1.d9) cut-off energy of electron (MeV)
 emin(13) = 1.000000000E-00 # (D=1.d9) cut-off energy of positron (MeV)
 emin(14) = 1.000000000E-03 # (D=1.d9) cut-off energy of photon (MeV)
 dmax(12) =  1000.00000     # (D=emin(12)) data max. energy of electron (MeV)
 dmax(13) =  1000.00000     # (D=emin(13)) data max. energy of positron (MeV)
 dmax(14) =  1000.00000     # (D=emin(14)) data max. energy of photon (MeV)
 igamma   =           1     # (D=0) photon emission from residual nuclei
 e-mode   =           1     # (D=0) 0: Normal, 1: Event generator mode
 nedisp   =           1     # (D=0) 0: no, 1: with e straggling, 10: ATIMA
 nspred   =           1     # (D=0) 0:no, 1:orig. 2:first Moliere, 10: ATIMA
 istdev   =          -1     # (D=1) 1:Batch variance, 2:History variance, <0:Restart mode
 file(6)  = phits.out       # (D=phits.out) general output file name
 file(7)  = c:/phits/data/xsdir.jnd # (D=xdirs) nuclear data input file name
 file(14) = c:/phits/data/trxcrd.dat # (D=trxcrd.dat) photon data input file name

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  11B            # kind of incident nucleus          
       e0 =   250.00        # energy of beam [MeV/u]
       r0 =   0.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -20.000        # minimum position of z-axis [cm]
       z1 =  -20.000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ] 
           1H        -1.1111100E-01
          16O        -8.8888900E-01
MAT[ 2 ] 
          27Al        1.0
MAT[ 5 ] 
           1H         6.8934070E-02
          16O         3.4467040E-02

[ C e l l ]
 1          1 -1.0000000E+00  1 -2 -11
 2          1 -2.7000000E+00  2 -3 -11 
98          0                 #(1 -3 -11) -999 
99         -1                 999 

[ S u r f a c e ]
 1            pz     0.0000000E+00
 2            pz     5.0000000E+00
 3            pz     1.0000000E+01
 11           cz     5.0000000E+00
 999          so     1.0000000E+02

[ T - Deposit ]
    title = depth-dose distribution
     mesh =  r-z            # mesh type is r-z scoring mesh
       x0 =   0.000000      # center x-position of r-z mesh
       y0 =   0.000000      # center y-position of r-z mesh
   r-type =    2            # r-mesh is linear given by rmin, rmax and nr
     rmin =   0.000000      # minimum value of r-mesh points
     rmax =   5.000000      # maximum value of r-mesh points
       nr =    5            # number of r-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =   0.000000      # minimum value of z-mesh points
     zmax =   10.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
     unit =    2            # unit is [MeV/source)]
   letmat =    0            # (D=0) material id for LET, 0: real material
  dedxfnc =    0            # (D=0) user defined multiplier, 0(no), 1, 2
 material =  all            # (D=all) number of specific material
   output =  dose           # total deposit energy
     axis =     z           # axis of output
     file = dose.out        # file name of output for the above axis
     part =  neutron proton He Li Be B
   epsout =    1

[ T - Deposit ] off
    title = depth-dose distribution for Q(L)-based dose equivalent
     mesh =  r-z            # mesh type is r-z scoring mesh
       x0 =   0.000000      # center x-position of r-z mesh
       y0 =   0.000000      # center y-position of r-z mesh
   r-type =    2            # r-mesh is linear given by rmin, rmax and nr
     rmin =   0.000000      # minimum value of r-mesh points
     rmax =   1.000000      # maximum value of r-mesh points
       nr =    1            # number of r-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =   0.000000      # minimum value of z-mesh points
     zmax =   10.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
     unit =    2            # unit is [MeV/source)]
   letmat =    5            # (D=0) material id for LET, 0: real material
  dedxfnc =    1            # (D=0) user defined multiplier, 0(no), 1, 2
 material =  all            # (D=all) number of specific material
   output =  dose           # total deposit energy
     axis =     z           # axis of output
     file = dose-equivalent.out  # file name of output for the above axis
     part =  all      
   epsout =    1

[ T - L E T ] off
    title = LET distribution in water
     mesh =  r-z            # mesh type is r-z scoring mesh
       x0 =   0.000000      # center x-position of r-z mesh
       y0 =   0.000000      # center y-position of r-z mesh
   r-type =    2            # r-mesh is linear given by rmin, rmax and nr
     rmin =   0.000000      # minimum value of r-mesh points
     rmax =   1.000000      # maximum value of r-mesh points
       nr =    1            # number of r-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =   0.000000      # minimum value of z-mesh points
     zmax =   10.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
   l-type =    3            # l-mesh is log given by lmin, lmax and nl
     lmin =  1.0000000E-02  # minimum value of l-mesh points
     lmax =   10000.00      # maximum value of l-mesh points
       nl =  180            # number of l-mesh points
     unit =    2            # unit is [MeV/(keV/um)/source]
   letmat =    5            # (D=0) material id for LET, 0: real material
 material =  all            # (D=all) number of specific material
     axis =  let            # axis of output
     file = let-distribution.out  # file name of output for the above axis
     part =  all      
   epsout =    1

[ T - S E D ] off
    title = Lineal-energy distribution in water
     mesh =  r-z            # mesh type is r-z scoring mesh
       x0 =   0.000000      # center x-position of r-z mesh
       y0 =   0.000000      # center y-position of r-z mesh
   r-type =    2            # r-mesh is linear given by rmin, rmax and nr
     rmin =   0.000000      # minimum value of r-mesh points
     rmax =   1.000000      # maximum value of r-mesh points
       nr =    1            # number of r-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =   0.000000      # minimum value of z-mesh points
     zmax =   10.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
  se-type =    3            # e-mesh is log given by emin, emax and ne
    emin  =  1.0000000E-02  # minimum value of e-mesh points
    emax  =   10000.00      # maximum value of e-mesh points
       ne =  180            # number of e-mesh points
  se-unit =    2            # se-unit is [keV/um]
     unit =    2            # unit is [MeV/z/source)]
    cdiam =  1.000000       # (D=1.0) diameter of SE cell [um]
   letmat =    5            # (D=0) material id for LET, 0: real material
 material =  all            # (D=all) number of specific material
     axis =  sed            # axis of output
     file = y-distribution.out  # file name of output for the above axis
     part =  all      
   epsout =    1

[END]

