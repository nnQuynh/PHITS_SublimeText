[ T i t l e ]
Sample input for SEU rate calculation

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =      100000     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 nedisp   =           1     # (D=0) 0: no, 1: with e straggling, 10: ATIMA
 nspred   =           2     # (D=0) 0:no, 1:original, 2:Lynch(Moliere), 10:ATIMA
 igchk    =           1     # (D=0) Check overlap region errors in CG and GG
 e-mode   =           2     # (D=0) 0: Normal, 1: Event generator mode Ver.1, 2: Ver.2
 negs     =           1     # (D=-1) =-1:original, =0:No, =1:EGS, =2:EGS(HE)
 irqmd    =           1     # (D=0) 0: JQMD legacy version, 1: JQMD-2.0
 mdbatima =        3000     # (D=500) max database size of ATIMA
 imevperu =           1     # (D=0) 0:[MeV] or 1:[MeV/n] is unit of tally output

[ S o u r c e ]
   s-type =   2             # rectangular-solid source
     proj =   4He           # kind of incident nucleus
       e0 =  10.0           # energy of beam [MeV/u]
       x0 =  -1.0e-4        # minimum position of x-axis [cm]
       x1 =   1.0e-4        # maximum position of x-axis [cm]
       y0 =  -1.0e-4        # minimum position of y-axis [cm]
       y1 =   1.0e-4        # maximum position of y-axis [cm]
       z0 =  -1.0           # minimum position of z-axis [cm]
       z1 =  -1.0           # maximum position of z-axis [cm]
      dir =   1.0           # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ] $ Silicon, 2.329 g/cm^3
    Si   1.0

MAT[ 2 ] $ SiO2, 2.65 g/cm^3
    Si   1.0
     O   2.0

MAT[ 3 ] $ Cu+SiO2, 5.69 g/cm^3
    Cu   3.0
    Si   1.0
     O   2.0

MAT[ 4 ] $ Epoxy, 1.85 g/cm^3
     C  18.0
     H  19.0
     O   5.0

[ C e l l ]
  99  -1   9999                               $ Outer void
  98   0  -9999   #(11 -12  13 -14   1  -7)   $ Inner void
   1   4  -1.85     11 -12  13 -14   1  -2    $ Package layer
   2   3  -5.69     11 -12  13 -14   2  -3    $ Metal layer
   3   2  -2.65     11 -12  13 -14   3  -4    $ Insulation layer
   4   2  -2.65     11 -12  13 -14   4  -5
                  #(21 -22  23 -24   4  -5)   $ STI
   5   1  -2.329    11 -12  13 -14   5  -7
                  #(21 -22  23 -24   5  -6)   $ Silicon substrate
  10   1  -2.329   (21 -22  23 -24   4  -6)   $ Sensitive volume

[ S u r f a c e ]
 9999  so   100.0

   11  px    -1.0e-4    $ xmin of Stacked target
   12  px     1.0e-4    $ xmax of Stacked target
   13  py    -1.0e-4    $ ymin of Stacked target
   14  py     1.0e-4    $ ymax of Stacked target

   21  px    -0.05e-4   $ xmin of Sensitive volume
   22  px     0.05e-4   $ xmax of Sensitive volume
   23  py    -0.10e-4   $ ymin of Sensitive volume
   24  py     0.10e-4   $ ymax of Sensitive volume

    1  pz  -503.35e-4   $ boundary of Inner void / Package layer
    2  pz    -3.35e-4   $ boundary of Package layer / Metal layer
    3  pz    -0.35e-4   $ boundary of Metal layer / Insulation layer
    4  pz     0.00e-4   $ boundary of Insulation layer / STI and Sensitive volume
    5  pz     0.25e-4   $ boundary of STI / Silicon substrate
    6  pz     0.50e-4   $ boundary of Sensitive volume / Silicon substrate
    7  pz   500.25e-4   $ boundary of Silicon substrate / Inner void

[ T - T r a c k ]
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin = -5.0e-4         # minimum value of x-mesh points
     xmax =  5.0e-4         # maximum value of x-mesh points
       nx =  100            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin = -5.0e-4         # minimum value of y-mesh points
     ymax =  5.0e-4         # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin = -5.0e-4         # minimum value of z-mesh points
     zmax =  5.0e-4         # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = ttrack-xz.out   # file name of output for the above axis
     part = all
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1.0e+6
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    3            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
    angel = cmum

