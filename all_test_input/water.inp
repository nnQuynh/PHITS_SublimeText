 file = water.inp

[ Title ]
Beam setting 1

[ Parameters ]
   icntl  = 0
   maxcas = 2000000
   maxbch = 10
  file(6) = phits.out

$ Optimized for Photon Therapy
 emin(2)  = 1.000000000E-10 # (D=1.0) cut-off energy of neutron (MeV)
 dmax(2)  =  20.0000000     # (D=emin(2)) data max. energy of neutron (MeV)
 emin(12) = 1.000000000E-01 # (D=1.d9) cut-off energy of electron (MeV)
 emin(13) = 1.000000000E-01 # (D=1.d9) cut-off energy of positron (MeV)
 emin(14) = 1.000000000E-03 # (D=1.d9) cut-off energy of photon (MeV)
 dmax(12) =  1000.00000     # (D=emin(12)) data max. energy of electron (MeV)
 dmax(13) =  1000.00000     # (D=emin(13)) data max. energy of positron (MeV)
 dmax(14) =  1000.00000     # (D=emin(14)) data max. energy of photon (MeV)
 igamma   =           2     # (D=0) 0:No, 1:Old, 2:EBITEM, 3:EBITEM+Isomer
 ipnint   =           1     # (D=0) photonuclear GDR, 1:analog, -1:implicit itall
$ Library data paths for file(7) & file(14)
 infl:{libpath.inp}

set: c91[     0.0    ] $ isocenter x
set: c92[     0.0    ] $ isocenter y
set: c93[     0.0    ] $ isocenter z
set: c94[   100.0    ] $ beam distance 
set: c95[    10.0    ] $ irradiation field x length
set: c96[    10.0    ] $ irradiation field z length

[ Source ]
 s-type = 5
     x0 =  c91
     x1 =  c91
     y0 =  c92-c94
     y1 =  c92-c94
     z0 =  c93
     z1 =  c93
    dir =  0.0
    phi =  90
    dom = asin(7.0/c94)*180.0/pi
   proj =  photon
 e-type = 1
     ne = 28
 infl:{spectrum.inp}

[ Surface ]
$ Outer region
   99 so 500
$ Colimator inside
   90 rpp c91-0.1*c95 c91+0.1*c95 c92-0.9*c94 c92-0.8*c94 c93-0.1*c96 c93+0.1*c96
$ Colimator outside
   91 rpp c91-0.5*c95 c91+0.5*c95 c92-0.9*c94 c92-0.8*c94 c93-0.5*c96 c93+0.5*c96
$ Main Space
   97 rpp -15.0 15.0 0 30.0 -15.0 15.0
$ Detector
   10 cy 1
   11 py c92+10.0-0.05
   12 py c92+10.0+0.05

[ Cell ]
$ Main space
   90 -1             90 -91        $ Void colimator 
   97  10 -1.0      -97 #10        $ Water
   10  10 -1.0      -10  11 -12    $ Detector
   98  1  -1.21e-3   97 -99 #90    $ Air
   99 -1         99                $ Outer region

[ Material ]
 MAT[1] $ Air 1.21e-3 g/cm3
           1H      7.199E-09
          12C      7.585E-09
          14N      3.910E-05
          16O      1.054E-05
 MAT[10] $ Water 1 g/cm3
           1H      2
          16O      1
 
[ Volume ]
  reg   vol
  10    pi*0.1

[ T-deposit ]
 title  = [t-deposit: icntl=0]
 file   = deposit.out
 mesh   = reg
 reg    = 10
 output = dose
 unit   = 0
 axis   = reg

[ T-deposit ]
 title  = [t-deposit: icntl=0]
 file   = deposit-y.out
 mesh   = xyz
 x-type = 2
 nx     = 1
 xmin   =-1.0
 xmax   = 1.0
 y-type = 2
 ny     = 30
 ymin   = 0.0
 ymax   = 30.0
 z-type = 2
 nz     = 1
 zmin   =-1.0
 zmax   = 1.0
 output = dose
 unit   = 0
 axis   = y
 epsout = 1

[ T-track ]
 title  = [t-track: icntl=0]
 file   = track-yz.out
 mesh   = xyz
 x-type = 2
 nx     =  1
 xmin   = c91-20
 xmax   = c91+20
 y-type = 2
 ny     =  100
 ymin   = c92-110
 ymax   = c92+10
 z-type = 2
 nz     =  40
 zmin   = c93-20
 zmax   = c93+20
 e-type = 2
 ne     = 1
 emin   = 0.0
 emax   = 1000
 axis   = yz
 gshow  = 1
 epsout = 1

[ T-track ]
 title  = [t-track: icntl=0]
 file   = track-xz.out
 mesh   = xyz
 x-type = 2
 nx     =  40
 xmin   = c91-10
 xmax   = c91+10
 y-type = 2
 ny     =    1
 ymin   = c92-0.1
 ymax   = c92+0.1
 z-type = 2
 nz     =  40
 zmin   = c93-10
 zmax   = c93+10
 e-type = 2
 ne     = 1
 emin   = 0.0
 emax   = 1000
 axis   = xz
 epsout = 1

[ End ]
