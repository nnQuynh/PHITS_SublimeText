[ T i t l e ]
sample input file of paraboloid by gq

[ P a r a m e t e r s ]
 icntl    =          11     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =          10     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 file(6)  = phits.out       # (D=phits.out) general output file name

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  proton         # kind of incident particle         
      dir =   1.0           # z-direction of beam [cosine]
       r0 =   0.            # radius [cm]
       z0 =   0.            # minimum position of z-axis [cm]
       z1 =   0.            # maximum position of z-axis [cm]
       e0 =   150.          # energy of beam [MeV/u]

[ M a t e r i a l ]
mat[1]    H 2  O 1     $ 1.0 g/cm3

[ S u r f a c e ]
set: c20[30]   $ angle of around Y (degree)
set: c21[cos(c20/180*pi)]
set: c22[sin(c20/180*pi)]
  10    gq     c21**2 1/2**2 c22**2  0. 0. -2*c21*c22  -c22 0 -c21  0
  11    pz     -10
  12  1 pz      10
  99    so      1000.

[ T r a n s f o r m ]
set: c10[0]   $ angle of around Z (degree)
set: c20[30]  $ angle of around Y (degree)
set: c30[0]   $ angle of around X (degree)

tr1   0 0 0
      cos(c10/180*pi)*cos(c20/180*pi)
      sin(c10/180*pi)*cos(c30/180*pi)+cos(c10/180*pi)*sin(c20/180*pi)*sin(c30/180*pi)
      sin(c10/180*pi)*sin(c30/180*pi)-cos(c10/180*pi)*sin(c20/180*pi)*cos(c30/180*pi)
     -sin(c10/180*pi)*cos(c20/180*pi)
      cos(c10/180*pi)*cos(c30/180*pi)-sin(c10/180*pi)*sin(c20/180*pi)*sin(c30/180*pi)
      cos(c10/180*pi)*sin(c30/180*pi)+sin(c10/180*pi)*sin(c20/180*pi)*cos(c30/180*pi)
      sin(c20/180*pi)
     -cos(c20/180*pi)*sin(c30/180*pi)
      cos(c20/180*pi)*cos(c30/180*pi)
      1

[ C e l l ]
 100     1 -1.         -10 11 -12
 200     0             -99 #100
 999    -1              99

[ T - 3Dshow ]
   output =    3            # (D=3) 0:draft, 1:line, 2:col, 3:line+col
       x0 =  0.             # (D=0.0) x-coordinate of the origin
       y0 =  0.             # (D=0.0) y-coordinate of the origin
       z0 =  0.            # (D=0.0) z-coordinate of the origin
    e-the =   60.           # (D=80.0) eye point theta(degree) from z-axis
    e-phi =   30.           # (D=140.0) eye point phi(degree) from x-axis
    e-dst =  120.           # (D=w-dst*10) eye point distance from origin
    l-the =   0.           # (D=e-the) light point theta from z-axis
    l-phi =   80.           # (D=e-phi) light point phi from x-axis
    l-dst =  100.           # (D=e-dst) light point distance from origin
    w-wdt =   50.           # (D=100) width of window (cm)
    w-hgt =   50.           # (D=100) hight of window (cm)
    w-dst =   30.           # (D=200) window distance from origin
   heaven =   z             # (D=y) direction to heaven
     line =    1            # (D=0) 0:surface+mat, 1:+region
   shadow =    2            # (D=0) 0:no, 1:shadow
    resol =    3            # (D=1) resolution of 3dshow
     file = 3dshow.out      # file name of output
    title = Check geometry using [T-3dshow] tally
   epsout =    1            # (D=0) generate eps file by ANGEL

[ T - T r a c k ]
    title = Track Detection in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
       nx =   50            # number of x-mesh points
     xmin =  -20.           # minimum value of x-mesh points
     xmax =   20.           # maximum value of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
       ny =   1            # number of y-mesh points
     ymin =  -20.           # minimum value of y-mesh points
     ymax =   20.           # maximum value of y-mesh points
   z-type =    2            # z-mesh is given by the below data
       nz =   50            # number of y-mesh points
     zmin =  -20.           # minimum value of y-mesh points
     zmax =   20.           # maximum value of y-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            0.0  5000.0
     unit =    1            # unit is [1/cm^2/source]
     axis =   xz            # axis of output
     file = track_xyz-xz.out  # file name of output for the above axis
     part =  all      
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
    width = 2
   epsout =    1            # (D=0) generate eps file by ANGEL

[ T - T r a c k ]
    title = Track Detection in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
       nx =   1            # number of x-mesh points
     xmin =  -20.           # minimum value of x-mesh points
     xmax =   20.           # maximum value of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
       ny =   50            # number of y-mesh points
     ymin =  -20.           # minimum value of y-mesh points
     ymax =   20.           # maximum value of y-mesh points
   z-type =    2            # z-mesh is given by the below data
       nz =   50            # number of y-mesh points
     zmin =  -20.           # minimum value of y-mesh points
     zmax =   20.           # maximum value of y-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            0.0  5000.0
     unit =    1            # unit is [1/cm^2/source]
     axis =   yz            # axis of output
     file = track_xyz-yz.out  # file name of output for the above axis
     part =  all      
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
    width = 2
   epsout =    1            # (D=0) generate eps file by ANGEL

[ E n d ]

