[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =        1000     # (D=10) number of particles per one batch
 maxbch   =           5     # (D=10) number of batches
 itall    =           1     # (D=0) 0:no tally at batch, 1:same, 2:different
 file(1)  = c:/phits        # (D=c:/phits) PHITS install folder name
 file(6)  = phits.out       # (D=phits.out) general output file name
 negs     =           1     # (D=-1) =-1:original, =0:No, =1:EGS

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  neutron        # kind of incident nucleus
       e0 =   100.000       # energy of beam [MeV/u]
       r0 =   0.0100        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -20.000        # minimum position of z-axis [cm]
       z1 =  -20.000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
$ Concrete D = 2.2 g/cm3
m1        H  1.3407e-2
          C  1.1030e-3
          O  4.3887e-2
         Al  1.7971e-3
         Si  1.6123e-2
         Ca  1.8940e-3
         Fe  3.3448e-4

$ Air D = 1.21e-3 g/cm3
m2         H     7.199E-09
           C     7.585E-09
           N     3.910E-05
           O     1.054E-05

$ Iron D = 7.7 g/cm3
m3        Fe    1.0

[ C e l l ]
 1          3 -7.7        1 -2 -11   $ Target
 2          1 -2.2        2 -3 -11   $ Void
98          2 -1.21e-3   #1 #2 -999  $ Void
99         -1             999  $ Outer region

[ S u r f a c e ]
set: c1[80.0]  $ Thickness of 1st Target (cm)
set: c2[25.0]  $ Thickness of 2nd Target (cm)
 1            pz     0.0
 2            pz      c1
 3            pz   c1+c2
 11           cz    50.0
 999          so   500.0

[ T - T r a c k ]
    title = Track in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -c1            # minimum value of x-mesh points
     xmax =   c1            # maximum value of x-mesh points
       nx =   50            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -c1            # minimum value of y-mesh points
     ymax =   c1            # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =   0.0           # minimum value of z-mesh points
     zmax =   c1*2.0        # maximum value of z-mesh points
       nz =   90            # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = track.out       # file name of output for the above axis
     part = electron photon alpha neutron
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1000.0
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat


[ T - T r a c k ] 
    title = Effective dose (uSv/h)
     part = ( neutron    photon   ) 
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -60.0          # minimum value of x-mesh points
     xmax =   60.0          # maximum value of x-mesh points
       nx =   1            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -60.0          # minimum value of y-mesh points
     ymax =   60.0          # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =    0.0          # minimum value of z-mesh points
     zmax =   c1+c2          # maximum value of z-mesh points
       nz =   60            # number of z-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            1.00000E-10  1.00000E+03
     unit =    1            # unit is [1/cm^2/source]
 material =  all            # (D=all) number of specific material
     axis =   z            # axis of output
     file =  effective_dose.out     # file name of output for the above axis
   epsout =    2            # (D=0) generate eps file by ANGEL
    y-txt =  Effective dose [uSv/h]

  multiplier = all          # number of material group
     part =  neutron  
     emax =    1000.0    
      mat      mset1  
      all ( 1.0 -102 )

  multiplier = all          # number of material group
     part =  photon   
     emax =    1000.0    
      mat      mset1  
      all ( 1.0  -114)

[END]
