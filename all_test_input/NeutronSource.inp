[ Title ]
Sample input for accelerator-based neutron source

[ Parameters ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =       10000     # (D=10) number of particles per one batch
 maxbch   =           2     # (D=10) number of batches
 dmax(1)  =  200.000000     # (D=emin(1)) data max. energy of proton (MeV)
 dmax(2)  =  200.000000     # (D=emin(2)) data max. energy of neutron (MeV)
 dmax(15) =  100.000000     # (D=emin(15)) data max. energy of deuteron (MeV/n)
 nfcseg   =          50     # (D=50) Number of segmentation in forced collision

[ Source ]
  totfact =   1.0000        # (D=1.0) global factor
   s-type =   1             # cylindrical source
     proj =  deuteron       # kind of incident particle         
       e0 =   20.000        # energy of beam [MeV/n]
       r0 =  0.25000        # radius [cm]
       r1 =   0.0000        # minimum radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -40.000        # minimum position of z-axis [cm]
       z1 =  -40.000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ Material ]
MAT[ 1 ] $ Be target
           Be         1.0000000E+00
MAT[ 2 ] $ 100MoO3 sample
        100Mo         1.0000000E+00
            O         3.0000000E+00

[ Surface ]
set: c11[2.0]     $ size of target [cm]
set: c12[0.9]     $ thickness of target [cm]
set: c13[-c12]    $ z-coordinate of target [cm]
set: c14[1.80]    $ density of target [g/cm3]
set: c15[0.55]    $ radius of sample [cm]
set: c16[0.179]   $ thickness of sample [cm]
set: c17[0.9]     $ z-coordinate of sample [cm]
set: c18[3.40]    $ density of sample [g/cm3]
set: c19[c18/(36.98)*6.02e23] $ particle density of 100MoO3 [#/cm^3]

  11  cz      c11
  12  pz      c13
  13  pz      c13+c12
  21  cz      c15
  22  pz      c17
  23  pz      c17+c16
 999  so      100.

[ Cell ]
   1     1 -c14  -11 12 -13  $ 9Be target
   2     2 -c18  -21 22 -23  $ 100MoO3 sample
 100     0       -999 #1 #2
 999    -1        999

[ Forced Collisions ]
   part = deuteron
   reg     fcl
     1    -1.0

[ T-Cross ]
    title = [t-cross] in region mesh
     mesh =  reg            # mesh type is region-wise
      reg =    1            # number of crossing regions
      non     r-from    r-to     area
      1       100       2       pi*c15**2
   e-type =    2            # e-mesh is linear given by emin, emax and ne
     emin =   0.000000      # minimum value of e-mesh points
     emax =   50.00000      # maximum value of e-mesh points
       ne =     50          # number of e-mesh points
     unit =    2            # unit is [1/cm^2/MeV/source]
     axis =  eng            # axis of output
     file = cross.out       # file name of output for the above axis
   output = flux            # surface crossing flux
     part =  neutron    proton     deuteron 
   epsout =    2            # (D=0) generate eps file by ANGEL

[ T-Track ]
    title = Track Detection in reg mesh
     mesh =  reg            # mesh type is region-wise
      reg = 2
   e-type =    2            # e-mesh is linear given by emin, emax and ne
     emin =   0.000000      # minimum value of e-mesh points
     emax =   50.00000      # maximum value of e-mesh points
       ne =      1          # number of e-mesh points
     unit =    4            # unit is [1/cm^2/source]
 material =  all            # (D=all) number of specific material
     axis =     reg            # axis of output
     file = yield-99Mo_multiplier.out  # file name of output for the above axis
     part =  neutron  
  multiplier = all          # number of material group
     part =  neutron  
     emax =    100.00    
      mat         mset1  
      all ( 1.0000 -299 )
   factor = 1e-27*c19       # (D=1.0) normalization factor
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat 5:bmp style
    resol =    1            # (D=1) resolution of gshow or rshow
    width = 0.5000000       # (D=0.5) width of lines for gshow or rshow
   epsout =    0            # (D=0) generate eps file by ANGEL

[ Multiplier ]
$ Activation cross sections of JENDL-4.0 (below 20 MeV) and
$ JENDL-4.0/HE (above 20 MeV) for 100Mo(n,2n)99Mo reaction
$ taken from https://wwwndc.jaea.go.jp/ftpnd/jendl/jendl40he_prod.html
     part =  all      
  number = -299
   interpolation = lin
  ne =   45
