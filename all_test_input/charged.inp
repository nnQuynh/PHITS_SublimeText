[ T i t l e ]
Sample input for adjoint mode for charged particles. Nuclear reaction is automatically prohibited (cmin(:)=1.0e9)

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =          10     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 nedisp   =           1     # (D=0) 0: no, 1: with e straggling, 10: ATIMA
 nspred   =           2     # (D=0) 0:no, 1:original, 2:Lynch(Moliere), 10:ATIMA
 iadjoint =           2     # (D=0) adjoint mode for photon (=1) or CP (=2)
 deltm    =        0.01     # (D=20.12345) should be small (<0.01) for max. flight mesh for transport (cm)
 imagnf   =           1     # (D=0) magnetic field
 imevperu =           1     # (D=0) 0:[MeV] or 1:[MeV/n] is unit of tally output

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  proton            # kind of incident nucleus          
       e0 =   0.1        # energy of beam [MeV/u]
       r0 =   0.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =   5.0000        # minimum position of z-axis [cm]
       z1 =   5.0000        # maximum position of z-axis [cm]
      dir =   all        # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ] 
            H        2.0
            O        1.0

[ Magnetic Field ]
   reg    typ    gap            mgf
     1      2     10.00000      50.00000    

[ C e l l ]
 1          1 -1.0   1 -2 -11  $ Water
98          0       #1 -999    $ Void
99         -1           999    $ Outer region

[ S u r f a c e ]
 1            pz     0.0
 2            pz    10.0
 11           cz     5.0
 999          so   100.0

[ T - T r a c k ]
    title = Track Detection in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
       nx =  150            # number of x-mesh points
     xmin =  -15.           # minimum value of x-mesh points
     xmax =   15.           # maximum value of x-mesh points
   y-type =    1            # y-mesh is given by the below data
       ny =    1            # number of y-mesh points
           -5.0  5.0
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
       nz =  150            # number of z-mesh points
     zmin =  -10.           # minimum value of z-mesh points
     zmax =   20.           # maximum value of z-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            0.0  5000.0
     unit =    1            # unit is [1/cm^2/source]
     axis =   xz            # axis of output
     file = track_xz.out    # file name of output for the above axis
     part =  all
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
   epsout =    1            # (D=0) generate eps file by ANGEL

[ T - C r o s s ]
    title = Energy distribution in region mesh
     mesh =  reg            # mesh type is region-wise
      reg =    1            # number of crossing regions
      non     r-from  r-to       area
        1     1       98      1.000000    
   e-type =    2            # e-mesh is linear given by emin, emax and ne
     emin =   0.0           # minimum value of e-mesh points
     emax =   250.          # maximum value of e-mesh points
       ne =   250            # number of e-mesh points
     unit =    1            # unit is [1/cm^2/source]
     axis =  eng            # axis of output
     file = cross_eng.out   # file name of output for the above axis
   output = flux            # surface crossing flux
     part =  all
   epsout =    1            # (D=0) generate eps file by ANGEL

