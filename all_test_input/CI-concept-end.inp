[ T i t l e ]
Sample input for shielding calculation using importance

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =           1     # (D=10) number of particles per one batch
 maxbch   =           1     # (D=10) number of batches
 negs     =           0     # (D=-1) =-1:original, =0:No, =1:EGS
 file(6)  = phits.out       # (D=phits.out) general output file name
 nrandgen =           0     # pseudo-random number generator option.

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  neutron        # kind of incident nucleus          
       e0 =   5.0         # energy of beam [MeV/u]
       r0 =   0.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -5.0000
       z1 =  -5.0000
      dir =  1            # inner direction with uniform dis. by analog

[ M a t e r i a l ]
$[m1 Concrete D=-2.2]
m1        H  1.3407e-2
          C  1.1030e-3
          O  4.3887e-2
         Al  1.7971e-3
         Si  1.6123e-2
         Ca  1.8940e-3
         Fe  3.3448e-4
$ Air  (density=1.2E-03 g/cm3)
MAT[ 2 ]
          H  -6.4000E-04
          C  -1.4000E-04
          N  -7.5086E-01
          O  -2.3555E-01
         Ar  -1.2810E-02

[ C e l l ]
 1      1 -2.2         1 -2 -40
 2      1 -2.2         2 -3 -40
 10     2 -1.2E-03 -99 #(1 -3 -40)
 11    -1           99 

[ S u r f a c e ]
set: c1[8.0]  $ cell thickness (cm)
 1            pz     0.0
 2            pz      c1
 3            pz      c1*2
40            cz      20

99            so     1000

[importance] 
part = neutron
reg imp
10  1.0
1   2.0
2   100.0

[ T - T r a c k ]
  mesh = xyz
x-type = 2
  xmin = -5
  xmax =  15
    nx = 200
z-type = 2
  zmin = -5
  zmax =  16
    nz = 200
y-type = 2
  ymin = -50
  ymax =  50
    ny = 1
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            1.00000E-10  1.00000E+03
     unit =    1            # unit is [1/cm^2/source]
 material =  all            # (D=all) number of specific material
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = importance-track.out  # file name of output for the above axis
     part =  neutron    
   epsout =    1  
    gshow =    3
angel=cmin(0.025) cmax(0.1)

[END]

