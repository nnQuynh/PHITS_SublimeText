[ T i t l e ]
Sample input for dose calculation for photon therapy

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =    10000000     # (D=10) number of particles per one batch
 maxbch   =           2     # (D=10) number of batches
 file(1)  = c:/phits        # (D=c:/phits) PHITS install folder name

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =   proton        # kind of incident nucleus          
       e0 =   10.000        # energy of beam [MeV/u]
       r0 =   0.1000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -20.000        # minimum position of z-axis [cm]
       z1 =  -20.000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
MAT[ 1 ]   # Beam pipe
           Al         1.0

MAT[ 2 ]   # Target
           Be         1.0

MAT[ 3 ]   # moderator
          2H          2.0
           O          1.0

MAT[ 4 ]   # reflector
           Be         1.0

MAT[ 5 ]   # shield
           B          4.0
           C          1.0

[ C e l l ]
 1          1 -2.7   1 -2 -101 100  203 $ Beam pipe
 2          2 -0.53  -200               $ Li target
 30         2 -1.00  -201 +200 -2010          $ Water moderator
 31         2 -1.00  -201 +200 -2011  2010    $ Water moderator
 32         2 -1.00  -201 +200 -2012  2011    $ Water moderator
 33         2 -1.00  -201 +200 -2013  2012    $ Water moderator
 34         2 -1.00  -201 +200 -2014  2013    $ Water moderator
 35         2 -1.00  -201 +200 -2015  2014    $ Water moderator
 36         2 -1.00  -201 +200 -2016  2015    $ Water moderator
 37         2 -1.00  -201 +200        2016    $ Water moderator
 40         2 -1.85  -202 +201 -2020            $ Be reflector
 41         2 -1.85  -202 +201 -2021  2020      $ Be reflector
 42         2 -1.85  -202 +201 -2022  2021      $ Be reflector
 43         2 -1.85  -202 +201 -2023  2022      $ Be reflector
 44         2 -1.85  -202 +201 -2024  2023      $ Be reflector
 45         2 -1.85  -202 +201 -2025  2024      $ Be reflector
 46         2 -1.85  -202 +201 -2026  2025      $ Be reflector
 47         2 -1.85  -202 +201        2026      $ Be reflector
 5          2 -2.52  -203 +202 +201 100 $ B4C Shield 
 6          0        -203 +202 +201 -100$ B4C Shield hole
98          0  #1 203    -999           $ Void
99         -1             999           $ Outer region

[ S u r f a c e ]
 1            pz   -20.0
 2            pz   150.0
 100          cz    10.0
 101          cz    15.0
 200          RPP   -5   5    -5    5   0   2 $ target
 201          RPP   -12  12   -12   12  0  40 $ moderator
 2010         PZ    5                         $ 
 2011         PZ   10                         $ 
 2012         PZ   15                         $ 
 2013         PZ   20                         $ 
 2014         PZ   25                         $ 
 2015         PZ   30                         $ 
 2016         PZ   35                         $ 
 202          RPP   -44  44   -44   44  0  39 $ reflector
 2020         RPP   -16  16   -16   16 -1  41 
 2021         RPP   -20  20   -20   20 -1  41 
 2022         RPP   -24  24   -24   24 -1  41 
 2023         RPP   -28  28   -28   28 -1  41 
 2024         RPP   -32  32   -32   32 -1  41 
 2025         RPP   -36  36   -36   36 -1  41 
 2026         RPP   -40  40   -40   40 -1  41 
 203          RPP   -45  45   -45   45 -1  40 $ Shield   
 999          so  1000.0

[I m p o r t a n c e ]
   set: c1[1.5]
   part  = neutron 
   reg     imp 
   30       c1**0
   31       c1**1
   32       c1**2
   33       c1**3
   34       c1**4
   35       c1**5
   36       c1**6
   37       c1**7
$
   set: c2[1.5]
   40       c2**0
   41       c2**(-1)
   42       c2**(-2)
   43       c2**(-3)
   44       c2**(-4)
   45       c2**(-5)
   46       c2**(-6)
   47       c2**(-7)

[ T - D e p o s i t ]
    title = Deposit in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin = -50.000000      # minimum value of x-mesh points
     xmax =  50.000000      # maximum value of x-mesh points
       nx =  50             # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin = -50.000000      # minimum value of y-mesh points
     ymax =  50.000000      # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -20.00000      # minimum value of z-mesh points
     zmax =  180.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
     unit =    0            # unit is [Gy/source]
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = deposit.out     # file name of output for the above axis
 material =  all            # (D=all) number of specific material
   output = dose            # only heat is written
   epsout =    1            # (D=0) generate eps file by ANGEL
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
    part  = proton  electron  nucleus

[ T - T r a c k ]
    title = Track in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin = -50.000000      # minimum value of x-mesh points
     xmax =  50.000000      # maximum value of x-mesh points
       nx =  50             # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin = -50.000000      # minimum value of y-mesh points
     ymax =  50.000000      # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -20.00000      # minimum value of z-mesh points
     zmax =  180.00000      # maximum value of z-mesh points
       nz =  100            # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = track.out       # file name of output for the above axis
     part = proton neutron photon
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1000.0
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat

[ T - C r o s s ]
    title = Energy distribution in region mesh
     mesh =  reg            # mesh type is region-wise
      reg =    1            # number of crossing regions
      non     r-from  r-to      area
        1      98        5      1.000000    
   e-type =    3            # e-mesh is log given by emin, emax and ne
     emin =   1.00e-10      # minimum value of e-mesh points
     emax =   15.00000      # maximum value of e-mesh points
       ne =   50            # number of e-mesh points
     unit =    3            # unit is [1/cm^2/MeV/source]
     axis =  eng            # axis of output
     file = cross_reg.out   # file name of output for the above axis
   output = flux            # surface crossing flux
     part =  neutron
   epsout =    1            # (D=0) generate eps file by ANGEL

[END]

