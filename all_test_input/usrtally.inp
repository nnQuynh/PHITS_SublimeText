[ T i t l e ]  
Sample input for calculating deposition energy distribution in semi-conductor device

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =         100     # (D=10) number of particles per one batch
 maxbch   =           2     # (D=10) number of batches
 emin(1)  = 1.000000000E-03 # (D=1.0) cut-off energy of proton (MeV)
 emin(2)  = 1.000000000E-10 # (D=1.0) cut-off energy of neutron (MeV)
 dmax(2)  =  20.0000000     # (D=emin(2)) data max. energy of neutron (MeV)
 emin(12) = 1.000000000E-01 # (D=1.d9) cut-off energy of electron (MeV)
 emin(13) = 1.000000000E-01 # (D=1.d9) cut-off energy of positron (MeV)
 emin(14) = 1.000000000E-03 # (D=1.d9) cut-off energy of photon (MeV)
 dmax(12) =  1000.00000     # (D=emin(12)) data max. energy of electron (MeV)
 dmax(13) =  1000.00000     # (D=emin(13)) data max. energy of positron (MeV)
 dmax(14) =  1000.00000     # (D=emin(14)) data max. energy of photon (MeV)
 e-mode   =           2     # (D=0) 0: Normal, 1: Event generator mode
 itall    =           1     # (D=0) 0:no tally at batch, 1:same, 2:different
 file(6)  = phits.out       # (D=phits.out) general output file name
 file(7)  = c:/phits/data/xsdir.jnd # (D=xdirs) nuclear data input file name
 file(14) = c:/phits/data/trxcrd.dat # (D=trxcrd.dat) photon data input file name

[ S o u r c e ] 
  totfact =   1.0000        # (D=1.0) global factor
   s-type =   1             # mono-energetic axial source
     proj =  proton         # kind of incident particle         
       e0 =   500.0         # energy of beam [MeV/u]
       r0 =   0.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -20.0000        # minimum position of z-axis [cm]
       z1 =  -20.0000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]
      dom =  1.0

[ M a t e r i a l ]
$   Target
m2        27Al         1.0


[ C e l l ]
    1     2 -2.7          1 -2  -11   $ Target
    2     0                -999 #1
    3    -1                 999 

[ S u r f a c e ]
    1   pz   -2.0
    2   pz    2.0
   11   cz    2.0
  999   so    100

[ T - P r o d u c t ]
    title = Particle production in reg mesh
     mesh =  reg            # mesh type is xyz scoring mesh
      reg =    1            # x-mesh is linear given by xmin, xmax and nx
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            0.  1e9
 material =  all            # (D=all) number of specific material
   mother =  all            # (D=all) number of specific mother
     unit =    1            # unit is [1/source]
     axis =  reg            # axis of output
     file = product.out  # file name of output for the above axis
   output = nuclear         # (D=nuclear) products from all reactions
     part =  all

[t-userdefined] 
file = userdefined.out

[END]

 
