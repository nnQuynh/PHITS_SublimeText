 file = Tetra_test1.inp

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =        1000     # (D=10) number of particles per one batch
 maxbch   =           1     # (D=10) number of batches
 ipnint   =           1     # (D=0) photonuclear GDR, 1:analog, -1:implicit itall
 itetauto = 1

[ S o u r c e ]
  totfact =  2.9959e+13     # 4.8 micro A as an example
   s-type =   1             # mono-energetic axial source
     proj =  proton         # kind of incident nucleus
       e0 =   1500.00       # energy of beam [MeV/u]
       r0 =   0.01          # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =   61.500        # minimum position of z-axis [cm]
       z1 =   61.500        # maximum position of z-axis [cm]
      dir =  -1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]

$ Chrome Steel( 7.730 g/cm3(680.15K) )
MAT[5002]  Cr   -8.55
        C    -0.096
        Mo   -0.94
        Mn   -0.39
        Si   -0.42
        N    -0.042
        Ni   -0.02
        Nb   -0.08
        V    -0.21
        O    -0.0026
        W    -0.01
        Fe  -89.2394

$ LBE( 10.2 g/cm3(680.15K) )
MAT[5001]  Pb  -44.5
        Bi  -55.5

[ M a t N a m e C o l o r]
mat    name      size      color
5002   Chrome    0.5       camel
5001   LBE       0.5       orangeyellow

[ C e l l ]
101    0             -20   u=1  lat=3  nfile=sample_phits_r1.bdf
  1    0             -10   fill=1
  2   -1              10

[ S u r f a c e ]
 10    rpp   -36.0  36.0   -36.0  36.0    0.0  120.0
 20    rpp   -40.0  40.0   -40.0  40.0   -5.0  125.0

[ T - Deposit ]
    title = Energy deposition in reg mesh
     file = Tetra_test1_deposit.out     # file name of output for the above axis
     mesh =  tet            # mesh type is xyz scoring mesh
      reg =  101
     unit =    5            # unit is [MeV/cm3/source]
     axis =  tet            # axis of output
   output =  dose           # total deposit energy
  foamout =  1

[ T - T r a c k ]
    title = Track in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -40            # minimum value of x-mesh points
     xmax =   40            # maximum value of x-mesh points
       nx =   80            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =   -5            # minimum value of y-mesh points
     ymax =    5            # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -5.0           # minimum value of z-mesh points
     zmax = 125.0           # maximum value of z-mesh points
       nz = 130             # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = Tetra_test1_trackXZ.out     # file name of output for the above axis
     part = proton neutron photon
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  2000.0
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
    resol =    1

[END]
