[ T i t l e ]
Sample input for shielding calculation using T-WWBG.

[ P a r a m e t e r s ]
 icntl    =          15     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =         100     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 negs     =          -1     # (D=-1) =-1:original, =0:No, =1:EGS
 iwwbias  =           1     # (D=0) WW Bias on(1) off(0)
 file(1)  = c:/phits        # (D=c:/phits) PHITS install folder name
 file(6)  = phits.out       # (D=phits.out) general output file name

$ infl: {wwbg.out}          # output file from [t-WWBG]
$ infl: {WWG.out}           # output file from [t-WWG]

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  neutron        # kind of incident nucleus          
       e0 =   14.00         # energy of beam [MeV/u]
       r0 =   1.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  90.1
       z1 =  90.1
      dir =  all

[ M a t e r i a l ]
m1     1001  1.3407e-2
       6012  1.1030e-3
       8016  4.3887e-2
      13027  1.7971e-3
      14028  1.6123e-2
      20040  1.8940e-3
      26056  3.3448e-4

[ C e l l ]
 1          1 -2.2   1 -2 -40
 2          1 -2.2   2 -3 -40
 3          1 -2.2   3 -4 -40
 4          1 -2.2   4 -5 -40
 5          1 -2.2   5 -6 -40
 6          1 -2.2   6 -7 -40
 7          1 -2.2   7 -8 -40
 8          1 -2.2   8 -9 -40
 9          1 -2.2   9 -10 -40
 10         1 -2.2   10 -11 -40
 11         1 -2.2   11 -12 -40
 12         1 -2.2   12 -13 -40

 40         0 -99  #(1 -13 -40)
 41          -1       99

[ S u r f a c e ]
set: c1[15.0]  $ cell thickness (cm)
 1            pz      0.0
 2            pz      c1
 3            pz      c1*2
 4            pz      c1*3
 5            pz      c1*4
 6            pz      c1*5
 7            pz      c1*6
 8            pz      c1*7
 9            pz      c1*8
 10           pz      c1*9
 11           pz      c1*10
 12           pz      c1*11
 13           pz      c1*12
 14           pz      c1*12+10
 40           cz      100
 99           so      500

[ V o l u m e ]
    reg        vol
  {1-12}    pi*100*100*c1

[ T - WWBG ]
     mesh =  reg            # mesh type is region-wise
      reg = { 1 - 12 }
     axis = wwbg            # axis of output
     file = wwbg.out        # file name of output for the above axis
     part =  neutron  
   e-type =    1            # e-mesh is given by the below data
       ne =      2          # number of e-mesh points
       0.0  1e-3  1.0
       x0 =  0.0            #  x of initial point
       y0 =  -20.0          #  y of initial point
       z0 =  50.0           #  z of initial point
       x1 =  0.0            #  x of final point
       y1 =  20.0           #  y of final point
       z1 =  150.0          #  z of final point
   n-mesh =      3          # number of region
   r-mesh =   10.0  10.0  10.0
   z-mesh =   10.0  10.0  10.0
   f-mesh =   1.0  0.5  0.1  0.05
    r-out =  1000.          # (D=0:no set) radius of outer sphere

[ T - WWG ] off
   mesh = reg               # mesh type is region-wise
   reg  = {1-12}
   part = neutron
   e-type = 1               # e-mesh is given by the below data
       ne = 1               # number of e-mesh points
     0    1e5
   axis   =  wwg            # axis of output
    file  =  WWG.out        # file name of output for the above axis

[END]

