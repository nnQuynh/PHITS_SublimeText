[Title]
Neutron dose rate from 400MeV proton incident reaction on Cu

[Parameters]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =        1000     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 maxbnk   =     1000000     # (D=10000) maximum bank memory length
 jmout    =           1     # (D=0) Density echo, 0:input, 1:number density
 e-mode   =           0     # (D=0) Event generator mode is not recommended for DCHAIN calculation

 set:c1[10]   $ thickness of Cu target
 set:c2[50]  $ thickness of concrete shield
 set:c3[15]  $ radius of ditector
 
[ S o u r c e ] 
   totfact = 250e3/400e6/1.602e-19
   s-type =   1             # axial source with energy spectrum
     proj =  proton         # kind of incident particle         
       r0 =   0.5           # radius [cm]
       z0 =  -100           # minimum position of z-axis [cm]
       z1 =  -100           # maximum position of z-axis [cm]
       x0  =    0
       y0 =   0.0
      dir =   1           # z-direction of beam [cosine]
         e0 = 400

[Material]
$   Concrete   2.302 g/cm^3, taken from ANL-5800 Type 02-a
$   Note: 2.302 g/cm^3 is US standard. 

m1       H     -0.023       
         C     -0.0023
         O     -1.22
         Na    -0.0368
         Mg    -0.005
         Al    -0.078
         Si    -0.775
         K     -0.0299
         Ca    -0.1
         Fe    -0.032

$   Air at ground level  1.21e-3 g/cm^3
m2        N   4.0586E-05
          O   1.0800E-05
         Ar   2.4255E-07

m3  Cu    1.0

[ Mat Name Color ]
    mat   name      color
     1    Concrete    gray
     2    Air         pastelblue
     3    Copper      red

[Surface] 
   1   rpp  -c1/2   c1/2     -c1/2    c1/2    -c1/2 c1/2
   2   cz    c1/2 
   3   pz   -c1/2
   4   pz   -200
   5   rpp  -200 200  -100     100    -200 200
   6   rpp  -200 200   100     100+c2 -200 200
   7   rpp  -200 200  -100-c2 -100    -200 200

  11   s    0 100+c2+c3 0 c3

   99  so  1000

[Cell] 
   1    3 -8.90          -1  
   2    0          -2 4  -3
   3    2 -1.21e-3 -5 #1 #2
   4    1 -2.302   -6 
   5    1 -2.302   -7
  11    2 -1.21e-3 -11

   99   2 -1.21e-3   -99  #1 #2 #3 #4 #5 #11
   999   -1           99

[volume]
reg vol
11   4/3*pi*c3*c3*c3
3    400*400*200-pi*(c1/2)**2*(400-c1/2)-c1**2

[T-Track] 
  title =  neutron distributions
    mesh = xyz
  y-type = 2
    ymin = -150
    ymax =  200+c2+50
      ny = 200
  z-type = 2 
    zmin = -200
    zmax =  200
      nz = 200
  x-type = 2
    xmin = -10
    xmax =  10
      nx =   1
  e-type = 2
    emin = 0
    emax =  1000
      ne = 1
  axis = yz
  file = yz-track.out
  part = neutron photon proton
  unit = 1
  gshow =    3   # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
  epsout = 1     # automatically generate eps plot 
  z-txt = Effective dose rate(Sv/sec)
multiplier =all
emax = 1000
$  (pSv/sec) (Sv/sec)
mat mset1 
all ( 1e-12 -202 ) 

   
[T-Track] 
  title =  energy spectrum at detector
    mesh = reg
     reg = 11
   e-type =    2            # e-mesh is linear given by emin, emax and ne
     emin =   0.0           # minimum value of e-mesh points
     emax =   400.0           # maximum value of e-mesh points
       ne =   100            # number of e-mesh points
  axis = eng
  file = detector-ene.out
  part = neutron
  unit = 1
   epsout = 1

[T-Track] 
  title =  dose rate at detector
    mesh = reg
    reg = 11 
  e-type = 2
    emin = 0
    emax =  1000
      ne = 1
  axis = reg
  file = dose-detector.out
  part = neutron
  unit = 1
multiplier =all
emax = 1000
$  (pSv/sec) (Sv/sec)
mat mset1 
all ( 1e-12 -202 ) 

[ T - D C H A I N ]
$  must section for DCHAIN 
    title = air activation
     mesh =  reg            # mesh type is region-wise
      reg =  3 
     file = air.out        # file name of dchain-sp input file
  timeevo =    2               # time evolution
    1.0 d  1.0
    1.0 d  0.0
  outtime =       10
      1.0000E+00 h
      2.0000E+00 h
      6.0000E+00 h
      1.2000E+01 h
      1.0000E+00 d
      2.5000E+01 h
      2.6000E+01 h
      3.1000E+01 h	  
      1.5000E+00 d
      2.000E+00 d
	  
$ set:c90[ 250e3 / (400e6*1.602177e-19) ]
$   amp = c90                 # (D=1.0) Source Intensity(source/sec)
$ The source unit is normalized by a factor in the Sources section.
   




[end]

