[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =        1000     # (D=10) number of particles per one batch
 maxbch   =           5     # (D=10) number of batches
 ipnint   =           1     # (D=0) photonuclear GDR, 1:analog, -1:implicit itall
 file(1)  = c:/phits        # (D=c:/phits) PHITS install folder name
 file(6)  = phits.out       # (D=phits.out) general output file name

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  proton         # kind of incident nucleus
       e0 =   200.000       # energy of beam [MeV/u]
       r0 =   0.01          # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -20.000        # minimum position of z-axis [cm]
       z1 =  -20.000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]

$ Air D = 1.21e-3 g/cm3
MAT[1]  1H   7.199E-09
        12C  7.585E-09
        14N  3.910E-05
        16O  1.054E-05

$ Tungsten D = 19.25 g/cm3
MAT[2]  W  1.0

$ Concrete D = 2.2 g/cm3
MAT[3]  1H    1.3407e-2
        12C   1.1030e-3
        16O   4.3887e-2
        27Al  1.7971e-3
        28Si  1.6123e-2
        40Ca  1.8940e-3
        56Fe  3.3448e-4

$ Iron D = 7.7 g/cm3
MAT[4]  Fe  1.0

[ M a t N a m e C o l o r]
mat    name      size      color
1      Air       1.0       white
2      Tungsten  0.5       green
3      Conc.     0.5       camel
4      Iron      0.5       gray

[ C e l l ]
999   -1             -998:999:900      $ Outer region
100    1 -1.21e-3    998    -1  -900 #20   $ Front Air
200    1 -1.21e-3     11  -999  -900   $ Behind Air
  1    4 -7.7      1    -2  -900   $ Wall 1
  2    4 -7.7      2    -3  -900   $ Wall 2
  3    4 -7.7      3    -4  -900   $ Wall 3
  4    4 -7.7      4    -5  -900   $ Wall 4
  5    4 -7.7      5    -6  -900   $ Wall 5
  6    3 -2.2      6    -7  -900   $ Wall 6
  7    3 -2.2      7    -8  -900   $ Wall 7
  8    3 -2.2      8    -9  -900   $ Wall 8
  9    3 -2.2      9   -10  -900   $ Wall 9
 10    3 -2.2     10   -11  -900   $ Wall 10
 20    2 -19.25       21 -22 -23       $ Tungsten target

[ S u r f a c e ]
 998    pz   -50.0 
 999    pz   200.0
 900    cz    50.0
 1      pz     0.0
 2      pz    10.0
 3      pz    20.0
 4      pz    30.0
 5      pz    40.0
 6      pz    50.0
 7      pz    60.0
 8      pz    70.0
 9      pz    80.0
 10     pz    90.0
 11     pz   100.0
 21     pz   -10.0
 22     pz    -5.0
 23     cz     5.0

[ V o l u m e ] 
$  must section for DCHAIN 
    reg   vol
    1   pi*10*50**2   
    2   pi*10*50**2   
    3   pi*10*50**2   
    4   pi*10*50**2   
    5   pi*10*50**2   
    6   pi*10*50**2   
    7   pi*10*50**2   
    8   pi*10*50**2   
    9   pi*10*50**2   
    10  pi*10*50**2   

[ T - T r a c k ]
    title = Track in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -50            # minimum value of x-mesh points
     xmax =   50            # maximum value of x-mesh points
       nx =   40            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -50            # minimum value of y-mesh points
     ymax =   50            # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin = -30.0           # minimum value of z-mesh points
     zmax = 120.0           # maximum value of z-mesh points
       nz =   60            # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xz            # axis of output
     file = trackXZ.out     # file name of output for the above axis
     part = proton neutron photon
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1000.0
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    2            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat

[ T - T r a c k ] 
    title = Track Z
     part = (neutron photon) neutron photon
     mesh =  r-z            # mesh type is r-z scoring mesh
   r-type =    2            # x-mesh is linear given by xmin, xmax and nx
     rmin =   0.0           # minimum value of x-mesh points
     rmax =  50.0           # maximum value of x-mesh points
       nr =    1            # number of x-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -5.0           # minimum value of z-mesh points
     zmax = 105.0           # maximum value of z-mesh points
       nz =   22            # number of z-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            1.00000E-10  1.00000E+03
     unit =    1            # unit is [1/cm^2/source]
 material =  all            # (D=all) number of specific material
     axis =   z             # axis of output
     file =  trackZ.out     # file name of output for the above axis
   epsout =    1            # (D=0) generate eps file by ANGEL
    y-txt =  Effective dose [Sv/h]
  multiplier = all          # number of material group
     part =  neutron photon 
     emax =    1000.0    
      mat      mset1 
      all (2.25e4 -202)
    angel = ymin(1.0e-2) ymax(1.0e3)

[ T - C r o s s ]
    title = Energy spectrum of neutron crossing surface
     mesh = reg             # mesh type is region-wise
      reg = 11              # number of crossing regions
      r-from r-to area
      100    1     1.0
      1      2     1.0
      2      3     1.0
      3      4     1.0
      4      5     1.0
      5      6     1.0
      6      7     1.0
      7      8     1.0
      8      9     1.0
      9      10    1.0
      10     200   1.0
     part = neutron proton
   e-type = 5               # e-mesh is log given by emin, emax and edel
     edel = log(10)/4       # width of e-mesh points
     emin = 1.0e-5          # minimum value of e-mesh points
     emax = 1.0e+3          # maximum value of e-mesh points
     axis = eng             # axis of output
     unit = 1               # unit is [1/cm^2/source]
   output = flux            # surface crossing flux
     file = cross.out       # file name of output for the above axis
   epsout = 1               # (D=0) generate eps file by ANGEL
    angel = ymin(1.0e-5) ymax(1.0e0)

[ T - D C H A I N ] off
$  must section for DCHAIN 
    title = Induced radiation
     mesh =  reg            # mesh type is region-wise
      reg =  5 6
     file = tdchain.out        # file name of dchain-sp input file
  timeevo =    2               # time evolution
    1.0 h  1.0
    50.0 y 0.0
  outtime =    6               # output time
    1.0 h
   10.0 y
   20.0 y
   30.0 y
   40.0 y
   50.0 y
$ beam current (nA)
set:c21[1000.0]
  amp = c21*1.0e-9/1.602e-19   # (D=1.0) Source Intensity(source/sec)

[ I m p o r t a n c e ]
set: c1[2.0]
     part = neutron photon
      reg imp
      100 c1**0
      1   c1**0
      2   c1**1
      3   c1**2
      4   c1**3
      5   c1**4
      6   c1**5
      7   c1**6
      8   c1**7
      9   c1**8
      10  c1**9
      200 c1**9

[END]
