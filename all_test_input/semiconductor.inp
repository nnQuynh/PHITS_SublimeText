[ T i t l e ]  
Sample input for calculating deposition energy distribution in semi-conductor device

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =     1000000     # (D=10) number of particles per one batch
 maxbch   =           2     # (D=10) number of batches
 esmin    = 1.000000000E-6  # (D=0.001) min energy for charged particle range 
 e-mode   =           2     # (D=0) 0: Normal, 1: Event generator mode
 negs     =           1     # (D=-1) =-1:original, =0:No, =1:EGS

[ S o u r c e ] 
  totfact =   1.0000        # (D=1.0) global factor
   s-type =   1             # mono-energetic axial source
     proj =  neutron        # kind of incident particle         
       e0 =   19.000        # energy of beam [MeV/u]
       r0 =   0.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  -1.0000        # minimum position of z-axis [cm]
       z1 =  -1.0000        # maximum position of z-axis [cm]
      dir =   1.0000        # z-direction of beam [cosine]

[ M a t e r i a l ]
m1  
       Si   1.0

[ C e l l ]
    1     1 -2.33                 1 -2  -11   $ Target
   98     0                      -999 #1
   99    -1                 999 

set: c20[10.0]   $ radius of sphere
set: c30[0.0003]  $ depth of sample
set: c40[0.0003]  $ radius of sample

[ S u r f a c e ]
    1   pz   -c30/2
    2   pz    c30/2
   11   cz    c40
  999   so     c20

[Volume]
reg vol
1   pi*c40*c40*c30

[ Delta Ray ]
    reg        del
     1        0.01

[T-deposit]
    title = [t-deposit] in region mesh
     mesh =  reg            # mesh type is region-wise
      reg = 1
     unit =    3            # unit is [1/source] : only for output=deposit
   letmat =    0            # (D=0) material id for LET, 0: real material
  dedxfnc =    0            # (D=0) user defined multiplier, 0(no), 1, 2
 material =  all            # (D=all) number of specific material
   output = deposit         # deposit enerygy distribution
   e-type =    3            # e-mesh is log given by emin, emax and ne
     emin =  1.0000000E-05  # minimum value of e-mesh points
     emax =   100.0000      # maximum value of e-mesh points
       ne =   50            # number of e-mesh points
     axis =   eng           # axis of output
     file = deposit.out     # file name of output for the above axis
     part =  all        Si         proton     deuteron   alpha    
   epsout =    1            # (D=0) generate eps file by ANGEL



[END]

 
