[ T - T r a c k ] 
    title = H*(10) or Effective dose (pSv/s)
     part = ( neutron    photon   ) 
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -60.0          # minimum value of x-mesh points
     xmax =   60.0          # maximum value of x-mesh points
       nx =   60            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -60.0          # minimum value of y-mesh points
     ymax =   60.0          # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =    0.0          # minimum value of z-mesh points
     zmax =  120.0          # maximum value of z-mesh points
       nz =   60            # number of z-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
            1.00000E-11  1.00000E+03
     unit =    1            # unit is [1/cm^2/source]
 material =  all            # (D=all) number of specific material
     axis =   xz            # axis of output
     file = track.out       # file name of output for the above axis
   epsout =    1            # (D=0) generate eps file by ANGEL
    z-txt = H*(10) or E [pSv/s]
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat

  multiplier = all          # number of material group
      mat      mset1      mset2        mset3
      all ( 1.0 -250 ) ( 1.0 -200 ) ( 1.0 -201 ) 
  $ 1st page: H*(10) calculated using [multiplier] defined in this input
  $ 2nd page: H*(10) calculated using pre-defined [multiplier] in data/multiplier/-200.inp
  $ 3rd page: Effective dose for AP calculated using pre-defined [multiplier] in data/multiplier/-201.inp

[ T - D e p o s i t ]
    title = Deposit in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -60.0          # minimum value of x-mesh points
     xmax =   60.0          # maximum value of x-mesh points
       nx =   60            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -60.0          # minimum value of y-mesh points
     ymax =   60.0          # maximum value of y-mesh points
       ny =    1            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =    0.0          # minimum value of z-mesh points
     zmax =  120.0          # maximum value of z-mesh points
       nz =   60            # number of z-mesh points
     unit =    2            # unit is [MeV/source]
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
     axis =   xz            # axis of output
     file = deposit.out        # file name of output for the above axis
 material =  all            # (D=all) number of specific material
   output =  dose           # total deposit energy
   epsout =    1            # (D=0) generate eps file by ANGEL

[ T - WWG ] off
      mesh = xyz
   x-type =  2
       nx =  4
     xmin =  -60
     xmax =   60
   y-type =  2
       ny =  4
     ymin = -60
     ymax =  60
   z-type =  2
       nz =  20
     zmin =  0
     zmax =  120
   part = all
   e-type = 1               # e-mesh is given by the below data
       ne = 1               # number of e-mesh points
     0      1e5
   axis   =  wwg            # axis of output
    file  =  WWG.out        # file name of output for the above axis

[END]
