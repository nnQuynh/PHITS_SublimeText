[ T i t l e ]
sample input file of t-rshow

[ P a r a m e t e r s ]
 icntl    =           9     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =          10     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 file(6)  = phits.out       # (D=phits.out) general output file name

[ S o u r c e ]
   s-type =   1             # mono-energetic axial source
     proj =  neutron        # kind of incident nucleus          
       e0 =   14.00         # energy of beam [MeV/u]
       r0 =   1.0000        # radius [cm]
       x0 =   0.0000        # (D=0.0) center position of x-axis [cm]
       y0 =   0.0000        # (D=0.0) center position of y-axis [cm]
       z0 =  90.1
       z1 =  90.1
      dir =  all

[ M a t e r i a l ]
m1     1001  1.3407e-2
       6012  1.1030e-3
       8016  4.3887e-2
      13027  1.7971e-3
      14028  1.6123e-2
      20040  1.8940e-3
      26056  3.3448e-4

[ C e l l ]
 1          1 -2.2   1 -2 -40
 2          1 -2.2   2 -3 -40
 3          1 -2.2   3 -4 -40
 4          1 -2.2   4 -5 -40
 5          1 -2.2   5 -6 -40
 6          1 -2.2   6 -7 -40
 7          1 -2.2   7 -8 -40
 8          1 -2.2   8 -9 -40
 9          1 -2.2   9 -10 -40
 10         1 -2.2   10 -11 -40
 11         1 -2.2   11 -12 -40
 12         1 -2.2   12 -13 -40

 40         0 -99  #(1 -13 -40)
 41          -1       99

[ S u r f a c e ]
set: c1[15.0]  $ cell thickness (cm)
 1            pz      0.0
 2            pz      c1
 3            pz      c1*2
 4            pz      c1*3
 5            pz      c1*4
 6            pz      c1*5
 7            pz      c1*6
 8            pz      c1*7
 9            pz      c1*8
 10           pz      c1*9
 11           pz      c1*10
 12           pz      c1*11
 13           pz      c1*12
 14           pz      c1*12+10
 40           cz      100
 99           so      500

[ V o l u m e ]
    reg        vol
  {1-12}    pi*100*100*c1

[ T - Rshow ]
    title = [t-rshow] in xyz mesh
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -120.0000      # minimum value of x-mesh points
     xmax =   120.0000      # maximum value of x-mesh points
       nx =  120            # number of x-mesh points
   y-type =    1            # y-mesh is given by the below data
       ny =      1          # number of y-mesh points
           -5.00000E+00  5.00000E+00
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -20.00000      # minimum value of z-mesh points
     zmax =   200.0000      # maximum value of z-mesh points
       nz =  110            # number of z-mesh points
     axis =   xz            # axis of output
     file = rshow_xyz-xz.out  # file name of output for the above axis
      reg = { 1 - 12 }
   value                    # values for each region
   non     reg      val     # reg definition
    1        1   0.0        # 1
    2        2   c1         # 2
    3        3   c1*2       # 3
    4        4   c1*3       # 4
    5        5   c1*4       # 5
    6        6   c1*5       # 6
    7        7   c1*6       # 7
    8        8   c1*7       # 8
    9        9   c1*8       # 9
   10       10   c1*9       # 10
   11       11   c1*10      # 11
   12       12   c1*11      # 12
   output =    3            # (D=1) 1:bnd, 2:bnd+num
    resol =    1            # (D=1) resolution of gshow or rshow
    width = 0.5000000       # (D=0.5) width of lines for gshow or rshow
   epsout =    1            # (D=0) generate eps file by ANGEL
    ginfo =    2            # (D=0) 0:no, 1:show in graph, 2:add file

[ E n d ]
