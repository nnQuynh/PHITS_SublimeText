[ T i t l e ]
Sample input file for cosmic-ray transport simulation

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =          50     # (D=10) number of particles per one batch
 maxbch   =           2     # (D=10) number of batches
 e-mode   =           2     # (D=0) 0: Normal, 1: Event generator mode Ver.1, 2: Ver.2
 irqmd    =           1     # (D=0) 0: JQMD legacy version, 1: JQMD-2.0
 negs     =           2     # (D=-1) =-1:original, =0:No, =1:EGS
 mdbatima =        3000     # (D=500) max database size of ATIMA
 maxbnk   =      100000     # (D=10000) maximum bank memory length
 iMeVperU =           1     # (D=0) 0:[MeV] or 1:[MeV/u] is unit of tally output
 esmax    =  100000000.     # (D=1.0e6) max energy/u for charged particle range
 emin(12) =  1.00000000     # (D=1.e+9) cut-off energy of electron (MeV)
 emin(13) =  1.00000000     # (D=1.e+9) cut-off energy of positron (MeV)

[ S o u r c e ]
set:c1[200.0]
  totfact =  -pi*c1**2       # (D=1.0) global factor
 <source> = 1.0
   s-type =   9             # mono-energetic axial source
     proj =  proton        # kind of incident nucleus          
       r1 =   c1             # radius of sphere [cm]
       r2 =   c1             # radius of source circle [cm]
      dir =  iso            # inner direction with uniform dis. by analog
   e-type =  25             # Cosmic-ray source
    icenv =   0             # 0>: Terrestiral GCR, D=0: Free-space GCR, <0:Free-space SEP
 solarmod =   0.0000        # Solar activity W-index
    rigid =   0.0000        # Cut-off rigidity in GV

 <source> = 1.0
   s-type =   9             # mono-energetic axial source
     proj =  56Fe        # kind of incident nucleus          
       r1 =   c1             # radius of sphere [cm]
       r2 =   c1             # radius of source circle [cm]
      dir =  iso            # inner direction with uniform dis. by analog
   e-type =  25             # Cosmic-ray source
    icenv =   0             # 0>: Terrestiral GCR, D=0: Free-space GCR, <0:Free-space SEP
 solarmod =   0.0000        # Solar activity W-index
    rigid =   0.0000        # Cut-off rigidity in GV

[ M a t e r i a l ]
MAT[ 1 ]  $ Water
           H  2.0  O  1.0
MAT[ 2 ]  $ Air
           N  0.8  O  0.2
MAT[ 3 ]  $ Al wall
           Al 1.0
MAT[ 5 ]  $ Water for LET calculation
           H  6.6872571e-2
           O  3.3436286e-2

[ C e l l ]
 1   1 -1.0     -1
 2   2 -1.0e-3  12 -13 -21 #1
 3   3 -2.7     11 -14 -22 #(12 -13 -21)
 98  0         -99         #(11 -14 -22)
 99 -1          99

[ S u r f a c e ]
 1  so   15.   $ Water radisu
11  pz  -60.
12  pz  -50.
13  pz   50.
14  pz   60.
21  cz   50.
22  cz   60.
99  so  1000.  

[ T - Deposit ]
    title = Energy deposition in reg mesh
     mesh =  reg            # mesh type is region-wise
      reg = 1
     unit =    0            
 material =  all            # (D=all) number of specific material
   output =  dose           # total deposit energy
     axis =   reg           # axis of output
     file = dose.out        # file name of output for the above axis
     part =  all

[ T - Deposit ]
    title = Energy deposition in reg mesh
     mesh =  reg            # mesh type is region-wise
      reg = 1
     unit =    0            
 material =  all            # (D=all) number of specific material
   output =  dose           # total deposit energy
     axis =   reg           # axis of output
     file = doseequivalent.out        # file name of output for the above axis
     part =  all
   letmat =   -5            # (D=0) mat ID for LET, 0:real mat, <0: electron for H2O
  dedxfnc =    1            # (D=0) user defined multiplier, 0(no), 1, 2

[ T - T r a c k ]
    title = Astronaut dose in pSv/source
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -60.            # minimum value of x-mesh points
     xmax =   60.            # maximum value of x-mesh points
       nx =   120            # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -60.           # minimum value of y-mesh points
     ymax =   60.            # maximum value of y-mesh points
       ny =   120            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -50            # minimum value of z-mesh points
     zmax =   50            # maximum value of z-mesh points
       nz =    1            # number of z-mesh points
  2D-type =    3            # 1:Cont, 2:Clust, 3:Color, 4:xyz, 5:mat, 6:Clust+Cont, 7:Col+Cont
     axis =   xy            # axis of output
     file = track.out       # file name of output for the above axis
     part = all proton neutron fe
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1e10
     unit =    1            # unit is [1/cm^2/source]
   epsout =    1            # (D=0) generate eps file by ANGEL   
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
    z-txt = [pSv/source]
 multiplier = all
   mat   mset1
   all  (1.0  -211 )      # -211 is for effective dose equivalent for male

[ T - T r a c k ]
    title = Track Detection in reg mesh
     mesh =  reg            # mesh type is region-wise
      reg =  (1 2)
   e-type =    3            # e-mesh is log given by emin, emax and ne
       ne =   120            # number of e-mesh points
     emin =   1.000000      # minimum value of e-mesh points
     emax =   1.0e6      # maximum value of e-mesh points
     unit =    2            # unit is [1/cm^2/MeV/source]
     axis =   eng            # axis of output
     file = track_eng.out  # file name of output for the above axis
     part =  proton neutron fe
    gshow =    1            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
   epsout =    1            # (D=0) generate eps file by ANGEL

[END]

