[ T i t l e ]
Sample input for SEU rate calculation

[ P a r a m e t e r s ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =        2000     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 nedisp   =           1     # (D=0) 0: no, 1: with e straggling, 10: ATIMA
 nspred   =           2     # (D=0) 0:no, 1:original, 2:Lynch(Moliere), 10:ATIMA
 igchk    =           1     # (D=0) Check overlap region errors in CG and GG
 e-mode   =           2     # (D=0) 0: Normal, 1: Event generator mode Ver.1, 2: Ver.2
 negs     =           1     # (D=-1) =-1:original, =0:No, =1:EGS, =2:EGS(HE)
 irqmd    =           1     # (D=0) 0: JQMD legacy version, 1: JQMD-2.0
 mdbatima =        3000     # (D=500) max database size of ATIMA
 imevperu =           1     # (D=0) 0:[MeV] or 1:[MeV/n] is unit of tally output

[ S o u r c e ]
  totfact =   pi*9.2**2
   s-type =   9             # spherical source
     proj =   4He           # kind of incident nucleus
       x0 =   0.0           # (D=0.0) center position of x-axis [cm]
       y0 =   0.0           # (D=0.0) center position of y-axis [cm]
       z0 =   0.0           # (D=0.0) center position of z-axis [cm]
       r1 =   9.2           # inner radius [cm]
       r2 =   9.2           # outer radius [cm]
      dir =  iso            # inner direction with uniform dis. by analog
   e-type =  25             # Cosmic-ray source
    icenv =   0             # 0>: Terrestiral GCR, D=0: Free-space GCR, <0:Free-space SEP
      eg1 =   8.0           # minimum cutoff energy [MeV/n]
      eg2 =   1.0e+5        # maximum cutoff energy [MeV/n]
    rigid =   0.0           # Cut-off rigidity in GV
   icyear =  2009
  icmonth =    10
    icday =    20

[ M a t e r i a l ]
MAT[ 1 ] $ Aluminum, 2.7 g/cm^3
    Al   1.0

MAT[ 2 ] $ Air, 1.21e-3 g/cm^3
     N   4.0586E-05
     O   1.0800E-05
    Ar   2.4255E-07

MAT[ 3 ] $ SiO2, 2.65 g/cm^3
    Si   1.0
     O   2.0

MAT[ 4 ] $ Li battery, Grahpite + LiCoO2, same volume ... ( 2.267 * 0.5 + 4.92 * 0.5 ) g/cm^3
     C   1.0*(2.26/12)
    Li   1.0*(4.92/97.87)
    Co   1.0*(4.92/97.87)
     O   2.0*(4.92/97.87)

MAT[ 5 ] $ SUS304, 8.03 g/cm^3
     C  -0.0006
    Si  -0.0080
     P  -0.0004
     S  -0.0002
    Cr  -0.1900
    Mn  -0.0180
    Fe  -0.6908
    Ni  -0.0920

MAT[ 6 ] $ Fe, 7.8658 g/cm^3
    Fe   1.0

MAT[ 7 ] $ Cu, 8.92 g/cm^3
    Cu   1.0

MAT[ 8 ] $ Si, 2.329 g/cm^3
    Si   1.0

[ C e l l ]
 9999  -1   9999                                           $ Outer void
 9998   0  -9999      10                                   $ Inner void
 9900   1  -2.7      -10  11  14                           $ Sattelite, wall
 9000   2  -1.21e-3  -11  12  14  20  30  40  52  53  54   $ Sattelite, room

  100   1  -2.7      -12  13  14                           $ Camera, flame 1
  102   2  -1.21e-3  -13  51                               $ Camera, room 1
  103   1  -2.7      -14  15  16                           $ Camera, flame 2
  104   2  -1.21e-3  -15  16  17                           $ Camera, room 2
  105   3  -2.65     -16                                   $ Camera, cover
  106   3  -2.65     -17                                   $ Camera, lens

  120   1  -2.7      -20  21                               $ Attitude control system flame
  121   2  -1.21e-3  -21  22  23  24                       $ Attitude control system room
  122   5  -8.03     -22
  123   5  -8.03     -23
  124   5  -8.03     -24

  130   4  -3.5935   -30                                   $ Li battery

  140   1  -2.7      -40  41                               $ Transformer flame
  141   2  -1.21e-3 (-41  42  44  45) : (-43  44  45)      $ Transformer room
  142   6  -7.8658   -42  43                               $ Transformer Fe core
  143   7  -8.92    (-44 : -45)  #(-42  43)                $ Transformer Cu coil

  151   8  -2.329    -51                                   $ Camera sensor circuit board
  152   8  -2.329    -52                                   $ Control board 1, for attitude control system
  153   8  -2.329    -53                                   $ Control board 2, for Li battery
  154   8  -2.329    -54                                   $ Control board 3, for transformer

[ S u r f a c e ]
set: c1[0.2]      $ thickness of sattelite wall
 9999  so    10.0                                                        $ outer void / inner void
   10  rpp   -4.8-c1   4.8+c1    -2.3-c1   2.3+c1    -4.8-c1   4.8+c1    $ Sattelite, wall
   11  rpp   -4.8      4.8       -2.3      2.3       -4.8      4.8       $ Sattelite, room

   12  rpp    0.3      4.7       -2.2      2.2        1.6      4.7       $ Camera, flame 1
   13  rpp    0.3+0.1  4.7-0.1   -2.2+0.1  2.2-0.1    1.6+0.1  4.7-0.1   $ Camera, room 1
   14  rcc    2.5  0.0 -5.0    0.0  0.0  6.7    2.0                      $ Camera, flame 2
   15  rcc    2.5  0.0 -5.0    0.0  0.0  6.7    2.0-0.1                  $ Camera, room 2
   16  rcc    2.5  0.0 -5.0    0.0  0.0  0.1    2.0-0.1                  $ Camera, cover
   17  rcc    2.5  0.0 -3.5    0.0  0.0  4.0    2.0-0.1                  $ Camera, lens

   20  rpp   -4.6     -2.4       -2.2      2.2       -4.6     -0.2       $ Attitude control system, flame
   21  rpp   -4.6+0.1 -2.4-0.1   -2.2+0.1  2.2-0.1   -4.6+0.1 -0.2-0.1   $ Attitude control system, room
   22  rcc   -3.5  1.0  -3.9   0.0  0.0  0.9    0.9                      $ Attitude control system, reaction wheel 1
   23  rcc   -3.5  0.5  -1.3   0.0  0.9  0.0    0.9                      $ Attitude control system, reaction wheel 2
   24  rcc   -4.0 -1.2  -1.3   0.9  0.0  0.0    0.9                      $ Attitude control system, reaction wheel 3

   30  rpp   -4.6     -0.2       -2.2      2.2        0.0  2.2           $ Li battery

   40  rpp   -4.6     -0.2       -2.2      2.2        2.3      4.5       $ Transformer, flame
   41  rpp   -4.6+0.1 -0.2-0.1   -2.2+0.1  2.2-0.1    2.3+0.1  4.5-0.1   $ Transformer, room
   42  rpp   -3.9     -0.9       -1.5      1.5        3.0      3.8       $ Transformer, Fe core
   43  rpp   -3.1     -1.7       -0.7      0.7        3.0      3.8       $ Transformer, Fe core hole
   44  rpp   -4.1     -2.9       -0.7      0.7        2.8      4.0       $ Transformer, Cu coil 1
   45  rpp   -1.9     -0.7       -0.7      0.7        2.8      4.0       $ Transformer, Cu coil 2

   51  rpp    0.5      4.5       -2.0      2.0        2.6      2.7       $ Camera sensor circuit board
   52  rpp   -1.7     -1.6       -2.0      2.0       -4.4     -0.4       $ Control board 1, for attitude control system
   53  rpp   -1.1     -1.0       -2.0      2.0       -4.4     -0.4       $ Control board 2, for Li battery
   54  rpp   -0.5     -0.4       -2.0      2.0       -4.4     -0.4       $ Control board 3, for transformer

[ M u l t i p l i e r ]
  number = -200
  interpolation = log
  part = 4He
  ne = 5
    8.0e+0   0.00e-0
    1.0e+1   1.64e-12
    2.5e+1   2.00e-13
    2.0e+2   8.00e-13
    2.0e+3   2.80e-13

[ T - T r a c k ]
     mesh =  xyz            # mesh type is xyz scoring mesh
   x-type =    2            # x-mesh is linear given by xmin, xmax and nx
     xmin =  -5.0           # minimum value of x-mesh points
     xmax =   5.0           # maximum value of x-mesh points
       nx =   100           # number of x-mesh points
   y-type =    2            # y-mesh is linear given by ymin, ymax and ny
     ymin =  -2.6           # minimum value of y-mesh points
     ymax =   2.6           # maximum value of y-mesh points
       ny =    5            # number of y-mesh points
   z-type =    2            # z-mesh is linear given by zmin, zmax and nz
     zmin =  -5.0           # minimum value of z-mesh points
     zmax =   5.0           # maximum value of z-mesh points
       nz =   100           # number of z-mesh points
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1.0e+6
     unit =    1            # unit is [1/cm^2/source]
     axis =   xz            # axis of output
     file = ttrack-xz.out   # file name of output for the above axis
     part = 4He
    gshow =    3            # 0: no 1:bnd, 2:bnd+mat, 3:bnd+reg 4:bnd+lat
   epsout =    1            # (D=0) generate eps file by ANGEL   
  multiplier = all
   mat  mset1
   all  (1  -200)

[ T - T r a c k ]
     mesh =  reg            # mesh type is xyz scoring mesh
      reg =  151  152  153  154
   volume
   reg   vol
   151   4.0*4.0*0.1
   152   4.0*4.0*0.1
   153   4.0*4.0*0.1
   154   4.0*4.0*0.1
   e-type =    1            # e-mesh is given by the below data
       ne =    1            # number of e-mesh points
             0.0  1.0e+6
     unit =    1            # unit is [1/cm^2/source]
     axis =   reg           # axis of output
     file = ttrack-reg.out  # file name of output for the above axis
     part = 4He
   epsout =    0            # (D=0) generate eps file by ANGEL   
  multiplier = all
   mat  mset1
   all  (1  -200)

[END]

